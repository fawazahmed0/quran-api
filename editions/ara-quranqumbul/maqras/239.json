{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ٱلَّذِي أَنقَضَ ظَهۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡر ࣰ ا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي أَحۡسَنِ تَقۡوِيم ࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدۡنَٰهُۥ أَسۡفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمُۥ أَجۡرٌ غَيۡرُ مَمۡنُون ࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّا إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبۡدًا إِذَا صَلَّىٰ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَة ࣲ كَٰذِبَةٍ خَاطِئَة ࣲ‏"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلۡيَدۡعُ نَادِيَهُۥ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّا لَا تُطِعۡهُۥ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّا أَنزَلۡنَٰهُۥ فِي لَيۡلَةِ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَا أَدۡرَىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡر ࣱ مِّنۡ أَلۡفِ شَهۡر ࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِمُۥ مِن كُلِّ أَمۡر ࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "لَمۡ يَكُنِ ٱلَّذِينَ كَفَرُواْ مِنۡ أَهۡلِ ٱلۡكِتَٰبِ وَٱلۡمُشۡرِكِينَ مُنفَكِّينَ حَتَّىٰ تَأۡتِيَهُمُ ٱلۡبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "رَسُول ࣱ مِّنَ ٱللَّهِ يَتۡلُواْ صُحُف ࣰ ا مُّطَهَّرَة ࣰ‏"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "فِيهَا كُتُب ࣱ قَيِّمَة ࣱ‏"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "وَمَا تَفَرَّقَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ إِلَّا مِنۢ بَعۡدِ مَا جَآءَتۡهُمُ ٱلۡبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "وَمَا أُمِرُواْ إِلَّا لِيَعۡبُدُواْ ٱللَّهَ مُخۡلِصِينَ لَهُ ٱلدِّينَ حُنَفَآءَ وَيُقِيمُواْ ٱلصَّلَوٰةَ وَيُؤۡتُواْ ٱلزَّكَوٰةَۚ وَذَٰلِكَ دِينُ ٱلۡقَيِّمَةِ"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "إِنَّ ٱلَّذِينَ كَفَرُواْ مِنۡ أَهۡلِ ٱلۡكِتَٰبِ وَٱلۡمُشۡرِكِينَ فِي نَارِ جَهَنَّمَ خَٰلِدِينَ فِيهَاۚ أُوْلَٰٓئِكَ هُمُۥ شَرُّ ٱلۡبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ أُوْلَٰٓئِكَ هُمُۥ خَيۡرُ ٱلۡبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "جَزَآؤُهُمُۥ عِندَ رَبِّهِمُۥ جَنَّٰتُ عَدۡن ࣲ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُ خَٰلِدِينَ فِيهَا أَبَد ࣰ اۖ رَّضِيَ ٱللَّهُ عَنۡهُمُۥ وَرَضُواْ عَنۡهُۥۚ ذَٰلِكَ لِمَنۡ خَشِيَ رَبَّهُۥ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "إِذَا زُلۡزِلَتِ ٱلۡأَرۡضُ زِلۡزَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "وَأَخۡرَجَتِ ٱلۡأَرۡضُ أَثۡقَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "وَقَالَ ٱلۡإِنسَٰنُ مَا لَهَا"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "يَوۡمَئِذ ࣲ تُحَدِّثُ أَخۡبَارَهَا"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "بِأَنَّ رَبَّكَ أَوۡحَىٰ لَهَا"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "يَوۡمَئِذ ࣲ يَصۡدُرُ ٱلنَّاسُ أَشۡتَات ࣰ ا لِّيُرَوۡاْ أَعۡمَٰلَهُمُۥ"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "فَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَيۡر ࣰ ا يَرَهُۥ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "وَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّة ࣲ شَرّ ࣰ ا يَرَهُۥ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَٱلۡمُورِيَٰتِ قَدۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَٱلۡمُغِيرَٰتِ صُبۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡع ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُود ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيد ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ"
        }
    ]
}