{
    "maqras": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "طهۚ"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "مَا أَنزَلۡنَا عَلَيۡكَ ٱلۡقُرَانَ لِتَشۡقَىٰ"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "إِلَّا تَذۡكِرَة ࣰ لِّمَن يَخۡشَىٰ"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "تَنزِيل ࣰ ا مِّمَّنۡ خَلَقَ ٱلۡأَرۡضَ وَٱلسَّمَٰوَٰتِ ٱلۡعُلَى"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "ٱلرَّحۡمَٰنُ عَلَى ٱلۡعَرۡشِ ٱسۡتَوَىٰ"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "لَهُۥ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا وَمَا تَحۡتَ ٱلثَّرَىٰ"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "وَإِن تَجۡهَرۡ بِٱلۡقَوۡلِ فَإِنَّهُۥ يَعۡلَمُ ٱلسِّرَّ وَأَخۡفَى"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "ٱللَّهُ لَا إِلَٰهَ إِلَّا هُوَۖ لَهُ ٱلۡأَسۡمَآءُ ٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "وَهَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "إِذۡ رَءَا نَار ࣰ ا فَقَالَ لِأَهۡلِهِ ٱمۡكُثُواْ إِنِّيَ ءَانَسۡتُ نَار ࣰ ا لَّعَلِّيَ ءَاتِيكُمُۥ مِنۡهَا بِقَبَسٍ أَوۡ أَجِدُ عَلَى ٱلنَّارِ هُد ࣰ ى"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "فَلَمَّا أَتَىٰهَا نُودِيَ يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "أَنِّيَ أَنَا۠ رَبُّكَ فَٱخۡلَعۡ نَعۡلَيۡكَ إِنَّكَ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوَىٰ"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "وَأَنَا ٱخۡتَرۡتُكَ فَٱسۡتَمِعۡ لِمَا يُوحَىٰ"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "إِنَّنِيَ أَنَا ٱللَّهُ لَا إِلَٰهَ إِلَّا أَنَا۠ فَٱعۡبُدۡنِي وَأَقِمِ ٱلصَّلَوٰةَ لِذِكۡرِي"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "إِنَّ ٱلسَّاعَةَ ءَاتِيَةٌ أَكَادُ أُخۡفِيهَا لِتُجۡزَىٰ كُلُّ نَفۡسِۭ بِمَا تَسۡعَىٰ"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "فَلَا يَصُدَّنَّكَ عَنۡهَا مَن لَّا يُؤۡمِنُ بِهَا وَٱتَّبَعَ هَوَىٰهُۥ فَتَرۡدَىٰ"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "وَمَا تِلۡكَ بِيَمِينِكَ يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "قَالَ هِيَ عَصَايَ أَتَوَكَّؤُاْ عَلَيۡهَا وَأَهُشُّ بِهَا عَلَىٰ غَنَمِي وَلِي فِيهَا مَـَٔارِبُ أُخۡرَىٰ"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "قَالَ أَلۡقِهَا يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "فَأَلۡقَىٰهَا فَإِذَا هِيَ حَيَّة ࣱ تَسۡعَىٰ"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "قَالَ خُذۡهَا وَلَا تَخَفۡۖ سَنُعِيدُهَا سِيرَتَهَا ٱلۡأُولَىٰ"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "وَٱضۡمُمۡ يَدَكَ إِلَىٰ جَنَاحِكَ تَخۡرُجۡ بَيۡضَآءَ مِنۡ غَيۡرِ سُوٓءٍ ءَايَةً أُخۡرَىٰ"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "لِنُرِيَكَ مِنۡ ءَايَٰتِنَا ٱلۡكُبۡرَى"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "قَالَ رَبِّ ٱشۡرَحۡ لِي صَدۡرِي"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "وَيَسِّرۡ لِي أَمۡرِي"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "وَٱحۡلُلۡ عُقۡدَة ࣰ مِّن لِّسَانِي"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "يَفۡقَهُواْ قَوۡلِي"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "وَٱجۡعَل لِّي وَزِير ࣰ ا مِّنۡ أَهۡلِي"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "هَٰرُونَ أَخِيَ"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ٱشۡدُدۡ بِهِۦ أَزۡرِي"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "وَأَشۡرِكۡهُۥ فِي أَمۡرِي"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "كَيۡ نُسَبِّحَكَ كَثِير ࣰ ا"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "وَنَذۡكُرَكَ كَثِيرًا"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "إِنَّكَ كُنتَ بِنَا بَصِير ࣰ ا"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "قَالَ قَدۡ أُوتِيتَ سُؤۡلَكَ يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "وَلَقَدۡ مَنَنَّا عَلَيۡكَ مَرَّةً أُخۡرَىٰ"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "إِذۡ أَوۡحَيۡنَا إِلَىٰ أُمِّكَ مَا يُوحَىٰ"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "أَنِ ٱقۡذِفِيهِۦ فِي ٱلتَّابُوتِ فَٱقۡذِفِيهِۦ فِي ٱلۡيَمِّ فَلۡيُلۡقِهِ ٱلۡيَمُّ بِٱلسَّاحِلِ يَأۡخُذۡهُۥ عَدُوّ ࣱ لِّي وَعَدُوّ ࣱ لَّهُۥۚ وَأَلۡقَيۡتُ عَلَيۡكَ مَحَبَّة ࣰ مِّنِّي وَلِتُصۡنَعَ عَلَىٰ عَيۡنِي"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "إِذۡ تَمۡشِي أُخۡتُكَ فَتَقُولُ هَلۡ أَدُلُّكُمُۥ عَلَىٰ مَن يَكۡفُلُهُۥۖ فَرَجَعۡنَٰكَ إِلَىٰ أُمِّكَ كَيۡ تَقَرَّ عَيۡنُهَا وَلَا تَحۡزَنَۚ وَقَتَلۡتَ نَفۡس ࣰ ا فَنَجَّيۡنَٰكَ مِنَ ٱلۡغَمِّ وَفَتَنَّٰكَ فُتُون ࣰ اۚ فَلَبِثۡتَ سِنِينَ فِي أَهۡلِ مَدۡيَنَ ثُمَّ جِئۡتَ عَلَىٰ قَدَر ࣲ يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "وَٱصۡطَنَعۡتُكَ لِنَفۡسِيَ"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "ٱذۡهَبۡ أَنتَ وَأَخُوكَ بِـَٔايَٰتِي وَلَا تَنِيَا فِي ذِكۡرِيَ"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "ٱذۡهَبَا إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "فَقُولَا لَهُۥ قَوۡل ࣰ ا لَّيِّن ࣰ ا لَّعَلَّهُۥ يَتَذَكَّرُ أَوۡ يَخۡشَىٰ"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "قَالَا رَبَّنَا إِنَّنَا نَخَافُ أَن يَفۡرُطَ عَلَيۡنَا أَوۡ أَن يَطۡغَىٰ"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "قَالَ لَا تَخَافَاۖ إِنَّنِي مَعَكُمَا أَسۡمَعُ وَأَرَىٰ"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "فَأۡتِيَاهُۥ فَقُولَا إِنَّا رَسُولَا رَبِّكَ فَأَرۡسِلۡ مَعَنَا بَنِي إِسۡرَٰٓءِيلَ وَلَا تُعَذِّبۡهُمُۥۖ قَدۡ جِئۡنَٰكَ بِـَٔايَة ࣲ مِّن رَّبِّكَۖ وَٱلسَّلَٰمُ عَلَىٰ مَنِ ٱتَّبَعَ ٱلۡهُدَىٰ"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "إِنَّا قَدۡ أُوحِيَ إِلَيۡنَا أَنَّ ٱلۡعَذَابَ عَلَىٰ مَن كَذَّبَ وَتَوَلَّىٰ"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "قَالَ فَمَن رَّبُّكُمَا يَٰمُوسَىٰ"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "قَالَ رَبُّنَا ٱلَّذِي أَعۡطَىٰ كُلَّ شَيۡءٍ خَلۡقَهُۥ ثُمَّ هَدَىٰ"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "قَالَ فَمَا بَالُ ٱلۡقُرُونِ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "قَالَ عِلۡمُهَا عِندَ رَبِّي فِي كِتَٰب ࣲ ۖ لَّا يَضِلُّ رَبِّي وَلَا يَنسَى"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "ٱلَّذِي جَعَلَ لَكُمُ ٱلۡأَرۡضَ مِهَٰد ࣰ ا وَسَلَكَ لَكُمُۥ فِيهَا سُبُل ࣰ ا وَأَنزَلَ مِنَ ٱلسَّمَآءِ مَآء ࣰ فَأَخۡرَجۡنَا بِهِۦ أَزۡوَٰج ࣰ ا مِّن نَّبَات ࣲ شَتَّىٰ"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "كُلُواْ وَٱرۡعَوۡاْ أَنۡعَٰمَكُمُۥۚ إِنَّ فِي ذَٰلِكَ لَأٓيَٰت ࣲ لِّأُوْلِي ٱلنُّهَىٰ"
        }
    ]
}