{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَٱلۡمُورِيَٰتِ قَدۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَٱلۡمُغِيرَٰتِ صُبۡح ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡع ࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُود ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيد ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُمُۥ بِهِمُۥ يَوۡمَئِذ ࣲ لَّخَبِيرُۢ"
        }
    ]
}