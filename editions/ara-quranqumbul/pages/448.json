{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "يَقُولُ أَ۟نَّكَ لَمِنَ ٱلۡمُصَدِّقِينَ"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "أَ۟ذَا مُتۡنَا وَكُنَّا تُرَاب ࣰ ا وَعِظَٰمًا أَ۟نَّا لَمَدِينُونَ"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "قَالَ هَلۡ أَنتُمُۥ مُطَّلِعُونَ"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "فَٱطَّلَعَ فَرَءَاهُۥ فِي سَوَآءِ ٱلۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرۡدِينِ"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "وَلَوۡلَا نِعۡمَةُ رَبِّي لَكُنتُ مِنَ ٱلۡمُحۡضَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "أَفَمَا نَحۡنُ بِمَيِّتِينَ"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "إِلَّا مَوۡتَتَنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "إِنَّ هَٰذَا لَهُوَ ٱلۡفَوۡزُ ٱلۡعَظِيمُ"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "لِمِثۡلِ هَٰذَا فَلۡيَعۡمَلِ ٱلۡعَٰمِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "أَذَٰلِكَ خَيۡر ࣱ نُّزُلًا أَمۡ شَجَرَةُ ٱلزَّقُّومِ"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "إِنَّا جَعَلۡنَٰهَا فِتۡنَة ࣰ لِّلظَّٰلِمِينَ"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "إِنَّهَا شَجَرَة ࣱ تَخۡرُجُ فِي أَصۡلِ ٱلۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "طَلۡعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَٰطِينِ"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "فَإِنَّهُمُۥ لَأٓكِلُونَ مِنۡهَا فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "ثُمَّ إِنَّ لَهُمُۥ عَلَيۡهَا لَشَوۡب ࣰ ا مِّنۡ حَمِيم ࣲ‏"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "ثُمَّ إِنَّ مَرۡجِعَهُمُۥ لَإِلَى ٱلۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "إِنَّهُمُۥ أَلۡفَوۡاْ ءَابَآءَهُمُۥ ضَآلِّينَ"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "فَهُمُۥ عَلَىٰ ءَاثَٰرِهِمُۥ يُهۡرَعُونَ"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "وَلَقَدۡ ضَلَّ قَبۡلَهُمُۥ أَكۡثَرُ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "وَلَقَدۡ أَرۡسَلۡنَا فِيهِمُۥ مُنذِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "فَٱنظُرۡ كَيۡفَ كَانَ عَٰقِبَةُ ٱلۡمُنذَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "وَلَقَدۡ نَادَىٰنَا نُوح ࣱ فَلَنِعۡمَ ٱلۡمُجِيبُونَ"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "وَنَجَّيۡنَٰهُۥ وَأَهۡلَهُۥ مِنَ ٱلۡكَرۡبِ ٱلۡعَظِيمِ"
        }
    ]
}