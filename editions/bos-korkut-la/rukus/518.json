{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O cemu oni jedne druge pitaju"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O vijesti velikoj"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "o kojoj oni razlicita misljenja imaju"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "To nije dobro, oni ce saznati sigurno"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "I jos jednom, to nije dobro, oni ce saznati sigurno"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Zar Zemlju posteljom nismo ucinili"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "i planine stubovima"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "i vas kao parove stvorili"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i san vas pocinkom ucinili"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "i noc pokrivacem dali"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "i dan za privređivanje odredili"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "i iznad vas sedam silnih sazidali"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "i svjetiljku plamtecu postavili"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Mi iz oblaka vodu obilno spustamo"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "i cinimo da uz njenu pomoc raste zito i bilje"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i basce guste"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Dan sudnji je, zaista, vec određen"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Dan kada ce se u rog puhnuti, pa cete vi, sve skupina po skupina, dolaziti"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "i nebo ce se otvoriti i mnogo kapija imati"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "i planine ce se zdrobiti i priviđenje ce biti"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Dzehennem ce zasjeda postati"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "nasilnicima mjesto povratka"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "u kome ce zauvijek ostati"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "u njemu svjezine nece osjetiti, ni pica okusiti"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "osim vrele vode i kapljevine"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "kazne prikladne"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Oni nisu ocekivali da ce racun polagati"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i dokaze Nase su pretjerano poricali"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "a Mi smo sve pobrojali i zapisali"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "pa trpite, mucenje cemo vam sve gorim uciniti"
        }
    ]
}