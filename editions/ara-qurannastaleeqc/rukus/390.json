{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "وَاِنَّ يُوۡنُسَ لَمِنَ الۡمُرۡسَلِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "اِذۡ اَبَقَ اِلَي الۡفُلۡكِ الۡمَشۡحُوۡنِ"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "فَسَاهَمَ فَكَانَ مِنَ الۡمُدۡحَضِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "فَالۡتَقَمَهُ الۡحُوۡتُ وَهُوَ مُلِيۡمٌ"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "فَلَوۡلَا٘ اَنَّهٗ كَانَ مِنَ الۡمُسَبِّحِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "لَلَبِثَ فِيۡ بَطۡنِهٖ٘ اِلٰي يَوۡمِ يُبۡعَثُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "فَنَبَذۡنٰهُ بِالۡعَرَآءِ وَهُوَ سَقِيۡمٌ"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "وَاَنۭۡبَتۡنَا عَلَيۡهِ شَجَرَةً مِّنۡ يَّقۡطِيۡنٍ"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "وَاَرۡسَلۡنٰهُ اِلٰي مِائَةِ اَلۡفٍ اَوۡ يَزِيۡدُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "فَاٰمَنُوۡا فَمَتَّعۡنٰهُمۡ اِلٰي حِيۡنٍ"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "فَاسۡتَفۡتِهِمۡ اَلِرَبِّكَ الۡبَنَاتُ وَلَهُمُ الۡبَنُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "اَمۡ خَلَقۡنَا الۡمَلٰٓئِكَةَ اِنَاثًا وَّهُمۡ شٰهِدُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "اَلَا٘ اِنَّهُمۡ مِّنۡ اِفۡكِهِمۡ لَيَقُوۡلُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "وَلَدَ اللّٰهُ وَاِنَّهُمۡ لَكٰذِبُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "اَصۡطَفَي الۡبَنَاتِ عَلَي الۡبَنِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "مَا لَكُمۡ كَيۡفَ تَحۡكُمُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "اَفَلَا تَذَكَّرُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "اَمۡ لَكُمۡ سُلۡطٰنٌ مُّبِيۡنٌ"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "فَاۡتُوۡا بِكِتٰبِكُمۡ اِنۡ كُنۡتُمۡ صٰدِقِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "وَجَعَلُوۡا بَيۡنَهٗ وَبَيۡنَ الۡجِنَّةِ نَسَبًا وَلَقَدۡ عَلِمَتِ الۡجِنَّةُ اِنَّهُمۡ لَمُحۡضَرُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "سُبۡحٰنَ اللّٰهِ عَمَّا يَصِفُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "اِلَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "فَاِنَّكُمۡ وَمَا تَعۡبُدُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "مَا٘ اَنۡتُمۡ عَلَيۡهِ بِفٰتِنِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "اِلَّا مَنۡ هُوَ صَالِ الۡجَحِيۡمِ"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "وَمَا مِنَّا٘ اِلَّا لَهٗ مَقَامٌ مَّعۡلُوۡمٌ"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "وَّاِنَّا لَنَحۡنُ الصَّآفُّوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "وَاِنَّا لَنَحۡنُ الۡمُسَبِّحُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "وَاِنۡ كَانُوۡا لَيَقُوۡلُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "لَوۡ اَنَّ عِنۡدَنَا ذِكۡرًا مِّنَ الۡاَوَّلِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "لَكُنَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "فَكَفَرُوۡا بِهٖ فَسَوۡفَ يَعۡلَمُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "وَلَقَدۡ سَبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا الۡمُرۡسَلِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "اِنَّهُمۡ لَهُمُ الۡمَنۡصُوۡرُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "وَاِنَّ جُنۡدَنَا لَهُمُ الۡغٰلِبُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "فَتَوَلَّ عَنۡهُمۡ حَتّٰي حِيۡنٍ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "وَّاَبۡصِرۡهُمۡ فَسَوۡفَ يُبۡصِرُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "اَفَبِعَذَابِنَا يَسۡتَعۡجِلُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "فَاِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَآءَ صَبَاحُ الۡمُنۡذَرِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "وَتَوَلَّ عَنۡهُمۡ حَتّٰي حِيۡنٍ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "وَّاَبۡصِرۡ فَسَوۡفَ يُبۡصِرُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "سُبۡحٰنَ رَبِّكَ رَبِّ الۡعِزَّةِ عَمَّا يَصِفُوۡنَ"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "وَسَلٰمٌ عَلَي الۡمُرۡسَلِيۡنَ"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "وَالۡحَمۡدُ لِلّٰهِ رَبِّ الۡعٰلَمِيۡنَ"
        }
    ]
