{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "ߊ߰ ߒ ߣߌ߫ ߥߎ߬ߛߎ ߕߍ߫"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ߤߊ߲߫ ߸ ߡߐ߱ ߦߋ߫ ߓߐ߬ߣߐ ߟߋ߬ ߘߐ߫ ߘߋ߬"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ߝߏ߫ ߡߍ߲ ߠߎ߬ ߞߍߣߍ߲߫ ߟߊߒߠߦߊߓߊ߮ ߘߌ߫ ߞߊ߬ ߢߌ߬ߡߊ ߟߎ߬ ߞߍ߫ ߸ ߊ߬ ߣߴߊ߬ߟߎ߫ ߞߊ߬ ߢߐ߲߯ ߡߊߛߊ߬ߙߊ߫ ߕߎ߬ߢߊ ߟߊ߫ ߸ ߊ߬ ߣߌ߫ ߞߊ߬ ߢߐ߲߯ ߡߊߛߊ߬ߙߊ߫ ߡߎ߬ߢߎ߲ ߠߊ߫"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "ߜߊ߬ߟߏ߬ ߦߋ߫ ߡߊ߬ߕߋ߰ߙߎ߬ߟߊ߫ ߡߊ߬ߓߙߎ߬ߕߌ߬ߟߊ ߞߋ߬ߟߋ߲߬ߞߋ߬ߟߋ߲߬ߠߊ ߦߋ߫"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ߡߍ߲ ߣߊ߬ ߣߊ߲߬ߝߟߏ ߟߊߘߍ߬ ߟߊ߫ ߞߵߊ߬ ߦߙߌߞߊ߫"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "ߊ߬ ߦߴߊ߬ ߢߣߊ߫ ߞߏ߫ ߊ߬ ߟߊ߫ ߣߊ߲߬ߝߟߏ ߟߋ߬ ߞߍߕߐ߫ ߊ߬ ߕߏ߫ ߟߊ߫ ( ߘߎߢߊ߫ ߘߐ߫)"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "ߞߊ߯ߙߌ߫ ߸ ߊ߬ ߓߍߣߊ߬ ߟߊߝߌ߬ߟߌ߫ ߘߐ߬ߓߍ߬ߘߍ߲߬ߛߍ߬ߟߌ߬ߟߊ ߘߐ߫ ߟߋ߬ ߘߍ߯"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "ߖߐ߲߫ ߞߵߌߟߋ ߟߊߟߐ߲ߠߌ߲߫ ߘߐ߬ߓߍ߬ߘߍ߲߬ߛߍ߬ߟߌ߬ߟߊ ߟߊ߫؟"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ߏ߬ ߦߋ߫ ߊߟߊ߫ ߟߊ߫ ߕߊ߫ ߟߊߡߍߣߍ߲ߣߍ߲߫ ߟߋ߬ ߘߌ߫"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ߡߍ߲ ߦߋ߫ ߥߊ߫ ߟߊ߫ ߛߌ߰ ߟߊ߫ ߛߐ߲߭ ߠߎ߫ ߞߊ߲߬"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "ߏ߬ ߘߊ ߜߊߛߏ߲ߕߐ߫ ߊ߬ߟߎ߫ ߡߊ߬ ߟߋ߬"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "ߛߡߊ߬ߛߋ߲߬ ߘߐߝߎߟߋ߲ߣߍ߲ ߘߐ߫ ߜߙߏߎ߫"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "ߌ ߡߴߊ߬ ߦߋ߫ ߓߊ߬ ߌ ߡߊ߰ߙߌ ߞߊ߬ ߡߍ߲ ߞߍ߫ ߛߋ߲߬ߓߊ߬ ߕߌ߮ ߟߎ߬ ߟߊ߫؟"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "ߊ߬ ߡߴߊ߬ߟߎ߬ ߟߊ߫ ߝߍ߬ߙߍ ߢߡߊߞߍ߫ ߊ߬ߟߎ߫ ߡߊ߬ ߓߊ߬؟"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ߞߣߐ߬ ߓߌ߮ ߗߋ߫ ߊ߬ߟߎ߬ ߞߊߡߊ߬"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ߏ߬ ߟߎ߫ ߞߵߊ߬ߟߎ߬ ߡߊߓߏ߲ߓߏ߲߫ ߣߊ߰ߘߌ߬ߝߘߊ߬ ߓߙߍ ߟߎ߬ ߟߊ߫"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "ߞߵߊ߬ߟߎ߬ ߞߍ߫ ߦߏ߫ ߢߊ߯ߡߊ-ߞߟߊ߫ ߢߌߡߌ߲ߣߍ߲ ߠߋ߬"
        }
    ]
