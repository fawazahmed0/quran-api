{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "ߊ߬ߓߎ߰-ߟߤߊߓߌ߲߫ ߓߟߏ߫ ߝߌ߬ߟߊ ߓߘߊ߫ ߕߏ߫ ߊ߬ ߘߐ߫ ߸ ߊ߬ ߖߍ߬ߘߍ ߓߘߊ߫ ߕߏ߫ ߊ߬ ߘߐ߫"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "ߊ߬ ߟߊ߫ ߣߊ߲߬ߝߟߏ ߣߴߊ߬ ߗߋߥߊߟߌ ߛߌ߫ ߕߍ߫ ߝߏߦߌ߫ ߢߵߊ߬ ߦߋ߫"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "ߊ߬ ߣߊ߬ ߘߏ߲߬ ߕߊ߫ ߡߍߣߍ߲ߠߊ ߘߐ߫߶"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ߊ߬ ߣߴߊ߬ ߡߛߏ߬ ߟߐ߯-ߞߎߘߎ߲߫ ߕߟߊ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "ߖߎ߬ߟߎ߫ ߞߊ߬ߦߌ߲߬ߣߍ߲ ߓߌ߬ߟߊ߬ߕߐ߫ ߏ߬ ߟߋ ߞߊ߲ߠߊ߫"
        }
    ]
