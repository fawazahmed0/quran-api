{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "۞وَأَوۡحَيۡنَآ إِلَىٰ مُوسَىٰٓ أَنۡ أَسۡرِ بِعِبَادِيٓ إِنَّكُم مُّتَّبَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَأَرۡسَلَ فِرۡعَوۡنُ فِي ٱلۡمَدَآئِنِ حَٰشِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "إِنَّ هَٰٓؤُلَآءِ لَشِرۡذِمَة ࣱ قَلِيلُونَ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَإِنَّهُمۡ لَنَا لَغَآئِظُونَ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَإِنَّا لَجَمِيعٌ حَٰذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَأَخۡرَجۡنَٰهُم مِّن جَنَّٰت ࣲ وَعُيُون ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَكُنُوز ࣲ وَمَقَام ࣲ كَرِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذَٰلِكَۖ وَأَوۡرَثۡنَٰهَا بَنِيٓ إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَأَتۡبَعُوهُم مُّشۡرِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا ٱلۡجَمۡعَانِ قَالَ أَصۡحَٰبُ مُوسَىٰٓ إِنَّا لَمُدۡرَكُونَ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّآۖ إِنَّ مَعِيَ رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوۡحَيۡنَآ إِلَىٰ مُوسَىٰٓ أَنِ ٱضۡرِب بِّعَصَاكَ ٱلۡبَحۡرَۖ فَٱنفَلَقَ فَكَانَ كُلُّ فِرۡق ࣲ كَٱلطَّوۡدِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزۡلَفۡنَا ثَمَّ ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيۡنَا مُوسَىٰ وَمَن مَّعَهُۥٓ أَجۡمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغۡرَقۡنَا ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَٱتۡلُ عَلَيۡهِمۡ نَبَأَ إِبۡرَٰهِيمَ"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذۡ قَالَ لِأَبِيهِ وَقَوۡمِهِۦ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُواْ نَعۡبُدُ أَصۡنَام ࣰ ا فَنَظَلُّ لَهَا عَٰكِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلۡ يَسۡمَعُونَكُمۡ إِذۡ تَدۡعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوۡ يَنفَعُونَكُمۡ أَوۡ يَضُرُّونَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُواْ بَلۡ وَجَدۡنَآ ءَابَآءَنَا كَذَٰلِكَ يَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَءَيۡتُم مَّا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمۡ وَءَابَآؤُكُمُ ٱلۡأَقۡدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمۡ عَدُوّ ࣱ لِّيٓ إِلَّا رَبَّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "ٱلَّذِي خَلَقَنِي فَهُوَ يَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَٱلَّذِي هُوَ يُطۡعِمُنِي وَيَسۡقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضۡتُ فَهُوَ يَشۡفِينِ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَٱلَّذِي يُمِيتُنِي ثُمَّ يُحۡيِينِ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَٱلَّذِيٓ أَطۡمَعُ أَن يَغۡفِرَ لِي خَطِيٓـَٔتِي يَوۡمَ ٱلدِّينِ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِي حُكۡم ࣰ ا وَأَلۡحِقۡنِي بِٱلصَّٰلِحِينَ"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَٱجۡعَل لِّي لِسَانَ صِدۡق ࣲ فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَٱجۡعَلۡنِي مِن وَرَثَةِ جَنَّةِ ٱلنَّعِيمِ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَٱغۡفِرۡ لِأَبِيٓ إِنَّهُۥ كَانَ مِنَ ٱلضَّآلِّينَ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخۡزِنِي يَوۡمَ يُبۡعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوۡمَ لَا يَنفَعُ مَال ࣱ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنۡ أَتَى ٱللَّهَ بِقَلۡب ࣲ سَلِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزۡلِفَتِ ٱلۡجَنَّةُ لِلۡمُتَّقِينَ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِلۡغَاوِينَ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيلَ لَهُمۡ أَيۡنَ مَا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِن دُونِ ٱللَّهِ هَلۡ يَنصُرُونَكُمۡ أَوۡ يَنتَصِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبۡكِبُواْ فِيهَا هُمۡ وَٱلۡغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبۡلِيسَ أَجۡمَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُواْ وَهُمۡ فِيهَا يَخۡتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَٱللَّهِ إِن كُنَّا لَفِي ضَلَٰل ࣲ مُّبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "إِذۡ نُسَوِّيكُم بِرَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَآ أَضَلَّنَآ إِلَّا ٱلۡمُجۡرِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِن شَٰفِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيقٍ حَمِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوۡ أَنَّ لَنَا كَرَّة ࣰ فَنَكُونَ مِنَ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتۡ قَوۡمُ نُوحٍ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ نُوحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِين ࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        }
    ]
}