{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "۞وَيَطُوفُ عَلَيۡهِمۡ وِلۡدَٰن ࣱ مُّخَلَّدُونَ إِذَا رَأَيۡتَهُمۡ حَسِبۡتَهُمۡ لُؤۡلُؤ ࣰ ا مَّنثُور ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "وَإِذَا رَأَيۡتَ ثَمَّ رَأَيۡتَ نَعِيم ࣰ ا وَمُلۡك ࣰ ا كَبِيرًا"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "عَٰلِيَهُمۡ ثِيَابُ سُندُسٍ خُضۡر ࣱ وَإِسۡتَبۡرَق ࣱ ۖ وَحُلُّوٓاْ أَسَاوِرَ مِن فِضَّة ࣲ وَسَقَىٰهُمۡ رَبُّهُمۡ شَرَاب ࣰ ا طَهُورًا"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "إِنَّ هَٰذَا كَانَ لَكُمۡ جَزَآء ࣰ وَكَانَ سَعۡيُكُم مَّشۡكُورًا"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "إِنَّا نَحۡنُ نَزَّلۡنَا عَلَيۡكَ ٱلۡقُرۡءَانَ تَنزِيل ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "فَٱصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تُطِعۡ مِنۡهُمۡ ءَاثِمًا أَوۡ كَفُور ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "وَٱذۡكُرِ ٱسۡمَ رَبِّكَ بُكۡرَة ࣰ وَأَصِيل ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "وَمِنَ ٱلَّيۡلِ فَٱسۡجُدۡ لَهُۥ وَسَبِّحۡهُ لَيۡل ࣰ ا طَوِيلًا"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "إِنَّ هَٰٓؤُلَآءِ يُحِبُّونَ ٱلۡعَاجِلَةَ وَيَذَرُونَ وَرَآءَهُمۡ يَوۡم ࣰ ا ثَقِيل ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "نَّحۡنُ خَلَقۡنَٰهُمۡ وَشَدَدۡنَآ أَسۡرَهُمۡۖ وَإِذَا شِئۡنَا بَدَّلۡنَآ أَمۡثَٰلَهُمۡ تَبۡدِيلًا"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "إِنَّ هَٰذِهِۦ تَذۡكِرَة ࣱ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيل ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُۚ إِنَّ ٱللَّهَ كَانَ عَلِيمًا حَكِيم ࣰ ا"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "يُدۡخِلُ مَن يَشَآءُ فِي رَحۡمَتِهِۦۚ وَٱلظَّٰلِمِينَ أَعَدَّ لَهُمۡ عَذَابًا أَلِيمَۢا"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "وَٱلۡمُرۡسَلَٰتِ عُرۡف ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فَٱلۡعَٰصِفَٰتِ عَصۡف ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "وَٱلنَّٰشِرَٰتِ نَشۡر ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فَٱلۡفَٰرِقَٰتِ فَرۡق ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عُذۡرًا أَوۡ نُذۡرًا"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِع ࣱ‏"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "لِيَوۡمِ ٱلۡفَصۡلِ"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآء ࣲ مَّهِين ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فَجَعَلۡنَٰهُ فِي قَرَار ࣲ مَّكِينٍ"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "إِلَىٰ قَدَر ࣲ مَّعۡلُوم ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "أَحۡيَآء ࣰ وَأَمۡوَٰت ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰت ࣲ وَأَسۡقَيۡنَٰكُم مَّآء ࣰ فُرَات ࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّ ࣲ ذِي ثَلَٰثِ شُعَب ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيل ࣲ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "إِنَّهَا تَرۡمِي بِشَرَر ࣲ كَٱلۡقَصۡرِ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَأَنَّهُۥ جِمَٰلَت ࣱ صُفۡر ࣱ‏"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَإِن كَانَ لَكُمۡ كَيۡد ࣱ فَكِيدُونِ"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰل ࣲ وَعُيُون ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "وَيۡل ࣱ يَوۡمَئِذ ࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ"
        }
    ]
}