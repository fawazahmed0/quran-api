{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَٱلنَّٰزِعَٰتِ غَرۡق ࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَٱلنَّٰشِطَٰتِ نَشۡط ࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَٱلسَّٰبِحَٰتِ سَبۡح ࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَٱلسَّٰبِقَٰتِ سَبۡق ࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡر ࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوب ࣱ يَوۡمَئِذ ࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَٰرُهَا خَٰشِعَة ࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَءِذَا كُنَّا عِظَٰم ࣰ ا نَّخِرَة ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُواْ تِلۡكَ إِذ ࣰ ا كَرَّةٌ خَاسِرَة ࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِيَ زَجۡرَة ࣱ وَٰحِدَة ࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصَىٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادَىٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَة ࣰ لِّمَن يَخۡشَىٰٓ"
        }
    ]
}