{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِين ࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِيعٍ ءَايَة ࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُم بَطَشۡتُمۡ جَبَّارِينَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَٱتَّقُواْ ٱلَّذِيٓ أَمَدَّكُم بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُم بِأَنۡعَٰم ࣲ وَبَنِينَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّٰت ࣲ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّيٓ أَخَافُ عَلَيۡكُمۡ عَذَابَ يَوۡمٍ عَظِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُواْ سَوَآءٌ عَلَيۡنَآ أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ ٱلۡوَٰعِظِينَ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنۡ هَٰذَآ إِلَّا خُلُقُ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهۡلَكۡنَٰهُمۡۚ إِنَّ فِي ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        }
    ]
}