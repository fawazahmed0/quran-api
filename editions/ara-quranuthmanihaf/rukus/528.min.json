{"rukus":[{"chapter":86,"verse":1,"text":"وَٱلسَّمَآءِ وَٱلطَّارِقِ"},{"chapter":86,"verse":2,"text":"وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"},{"chapter":86,"verse":3,"text":"ٱلنَّجۡمُ ٱلثَّاقِبُ"},{"chapter":86,"verse":4,"text":"إِن كُلُّ نَفۡس ࣲ لَّمَّا عَلَيۡهَا حَافِظ ࣱ‏"},{"chapter":86,"verse":5,"text":"فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"},{"chapter":86,"verse":6,"text":"خُلِقَ مِن مَّآء ࣲ دَافِق ࣲ‏"},{"chapter":86,"verse":7,"text":"يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"},{"chapter":86,"verse":8,"text":"إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِر ࣱ‏"},{"chapter":86,"verse":9,"text":"يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"},{"chapter":86,"verse":10,"text":"فَمَا لَهُۥ مِن قُوَّة ࣲ وَلَا نَاصِر ࣲ‏"},{"chapter":86,"verse":11,"text":"وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"},{"chapter":86,"verse":12,"text":"وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"},{"chapter":86,"verse":13,"text":"إِنَّهُۥ لَقَوۡل ࣱ فَصۡل ࣱ‏"},{"chapter":86,"verse":14,"text":"وَمَا هُوَ بِٱلۡهَزۡلِ"},{"chapter":86,"verse":15,"text":"إِنَّهُمۡ يَكِيدُونَ كَيۡد ࣰ ا"},{"chapter":86,"verse":16,"text":"وَأَكِيدُ كَيۡد ࣰ ا"},{"chapter":86,"verse":17,"text":"فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"}]}