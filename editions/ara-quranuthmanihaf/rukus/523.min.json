{"rukus":[{"chapter":81,"verse":1,"text":"إِذَا ٱلشَّمۡسُ كُوِّرَتۡ"},{"chapter":81,"verse":2,"text":"وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ"},{"chapter":81,"verse":3,"text":"وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ"},{"chapter":81,"verse":4,"text":"وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ"},{"chapter":81,"verse":5,"text":"وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ"},{"chapter":81,"verse":6,"text":"وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ"},{"chapter":81,"verse":7,"text":"وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ"},{"chapter":81,"verse":8,"text":"وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ"},{"chapter":81,"verse":9,"text":"بِأَيِّ ذَنۢبࣲ قُتِلَتۡ"},{"chapter":81,"verse":10,"text":"وَإِذَا ٱلصُّحُفُ نُشِرَتۡ"},{"chapter":81,"verse":11,"text":"وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ"},{"chapter":81,"verse":12,"text":"وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ"},{"chapter":81,"verse":13,"text":"وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ"},{"chapter":81,"verse":14,"text":"عَلِمَتۡ نَفۡسࣱ مَّآ أَحۡضَرَتۡ"},{"chapter":81,"verse":15,"text":"فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ"},{"chapter":81,"verse":16,"text":"ٱلۡجَوَارِ ٱلۡكُنَّسِ"},{"chapter":81,"verse":17,"text":"وَٱلَّيۡلِ إِذَا عَسۡعَسَ"},{"chapter":81,"verse":18,"text":"وَٱلصُّبۡحِ إِذَا تَنَفَّسَ"},{"chapter":81,"verse":19,"text":"إِنَّهُۥ لَقَوۡلُ رَسُولࣲ كَرِيمࣲ‏"},{"chapter":81,"verse":20,"text":"ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينࣲ‏"},{"chapter":81,"verse":21,"text":"مُّطَاعࣲ ثَمَّ أَمِينࣲ‏"},{"chapter":81,"verse":22,"text":"وَمَا صَاحِبُكُم بِمَجۡنُونࣲ‏"},{"chapter":81,"verse":23,"text":"وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ"},{"chapter":81,"verse":24,"text":"وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينࣲ‏"},{"chapter":81,"verse":25,"text":"وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنࣲ رَّجِيمࣲ‏"},{"chapter":81,"verse":26,"text":"فَأَيۡنَ تَذۡهَبُونَ"},{"chapter":81,"verse":27,"text":"إِنۡ هُوَ إِلَّا ذِكۡرࣱ لِّلۡعَٰلَمِينَ"},{"chapter":81,"verse":28,"text":"لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ"},{"chapter":81,"verse":29,"text":"وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ"}]}