{"pages":[{"chapter":85,"verse":1,"text":"والسماء ذات البروج"},{"chapter":85,"verse":2,"text":"واليوم الموعود"},{"chapter":85,"verse":3,"text":"وشاهد ومشهود"},{"chapter":85,"verse":4,"text":"قتل اصحب الاخدود"},{"chapter":85,"verse":5,"text":"النار ذات الوقود"},{"chapter":85,"verse":6,"text":"اذ هم عليها قعود"},{"chapter":85,"verse":7,"text":"وهم على ما يفعلون بالمومنين شهود"},{"chapter":85,"verse":8,"text":"وما نقموا منهم الا ان يومنوا بالله العزيز الحميد"},{"chapter":85,"verse":9,"text":"الذي له ملك السموت والارض والله على كل شيء شهيد"},{"chapter":85,"verse":10,"text":"ان الذين فتنوا المومنين والمومنت ثم لم يتوبوا فلهم عذاب جهنم ولهم عذاب الحريق"},{"chapter":85,"verse":11,"text":"ان الذين ءامنوا وعملوا الصلحت لهم جنت تجري من تحتها الانهر ذلك الفوز الكبير"},{"chapter":85,"verse":12,"text":"ان بطش ربك لشديد"},{"chapter":85,"verse":13,"text":"انه هو يبدي ويعيد"},{"chapter":85,"verse":14,"text":"وهو الغفور الودود"},{"chapter":85,"verse":15,"text":"ذو العرش المجيد"},{"chapter":85,"verse":16,"text":"فعال لما يريد"},{"chapter":85,"verse":17,"text":"هل اتىك حديث الجنود"},{"chapter":85,"verse":18,"text":"فرعون وثمود"},{"chapter":85,"verse":19,"text":"بل الذين كفروا في تكذيب"},{"chapter":85,"verse":20,"text":"والله من ورايهم محيط"},{"chapter":85,"verse":21,"text":"بل هو قرءان مجيد"},{"chapter":85,"verse":22,"text":"في لوح محفوظ"}]}