{"pages":[{"chapter":69,"verse":35,"text":"فليس له اليوم ههنا حميم"},{"chapter":69,"verse":36,"text":"ولا طعام الا من غسلين"},{"chapter":69,"verse":37,"text":"لا ياكله الا الخطون"},{"chapter":69,"verse":38,"text":"فلا اقسم بما تبصرون"},{"chapter":69,"verse":39,"text":"وما لا تبصرون"},{"chapter":69,"verse":40,"text":"انه لقول رسول كريم"},{"chapter":69,"verse":41,"text":"وما هو بقول شاعر قليل ا ما تومنون"},{"chapter":69,"verse":42,"text":"ولا بقول كاهن قليل ا ما تذكرون"},{"chapter":69,"verse":43,"text":"تنزيل من رب العلمين"},{"chapter":69,"verse":44,"text":"ولو تقول علينا بعض الاقاويل"},{"chapter":69,"verse":45,"text":"لاخذنا منه باليمين"},{"chapter":69,"verse":46,"text":"ثم لقطعنا منه الوتين"},{"chapter":69,"verse":47,"text":"فما منكم من احد عنه حجزين"},{"chapter":69,"verse":48,"text":"وانه لتذكرة للمتقين"},{"chapter":69,"verse":49,"text":"وانا لنعلم ان منكم مكذبين"},{"chapter":69,"verse":50,"text":"وانه لحسرة على الكفرين"},{"chapter":69,"verse":51,"text":"وانه لحق اليقين"},{"chapter":69,"verse":52,"text":"فسبح باسم ربك العظيم"},{"chapter":70,"verse":1,"text":"سال سايل بعذاب واقع"},{"chapter":70,"verse":2,"text":"للكفرين ليس له دافع"},{"chapter":70,"verse":3,"text":"من الله ذي المعارج"},{"chapter":70,"verse":4,"text":"تعرج المليكة والروح اليه في يوم كان مقداره خمسين الف سنة"},{"chapter":70,"verse":5,"text":"فاصبر صبر ا جميلا"},{"chapter":70,"verse":6,"text":"انهم يرونه بعيد ا"},{"chapter":70,"verse":7,"text":"ونرىه قريب ا"},{"chapter":70,"verse":8,"text":"يوم تكون السماء كالمهل"},{"chapter":70,"verse":9,"text":"وتكون الجبال كالعهن"},{"chapter":70,"verse":10,"text":"ولا يسل حميم حميم ا"}]}