{"pages":[{"chapter":70,"verse":11,"text":"يبصرونهم يود المجرم لو يفتدي من عذاب يوميذ ببنيه"},{"chapter":70,"verse":12,"text":"وصحبته واخيه"},{"chapter":70,"verse":13,"text":"وفصيلته التي تويه"},{"chapter":70,"verse":14,"text":"ومن في الارض جميع ا ثم ينجيه"},{"chapter":70,"verse":15,"text":"كلا انها لظى"},{"chapter":70,"verse":16,"text":"نزاعة للشوى"},{"chapter":70,"verse":17,"text":"تدعوا من ادبر وتولى"},{"chapter":70,"verse":18,"text":"وجمع فاوعى"},{"chapter":70,"verse":19,"text":"۞ان الانسن خلق هلوعا"},{"chapter":70,"verse":20,"text":"اذا مسه الشر جزوع ا"},{"chapter":70,"verse":21,"text":"واذا مسه الخير منوعا"},{"chapter":70,"verse":22,"text":"الا المصلين"},{"chapter":70,"verse":23,"text":"الذين هم على صلاتهم دايمون"},{"chapter":70,"verse":24,"text":"والذين في امولهم حق معلوم"},{"chapter":70,"verse":25,"text":"للسايل والمحروم"},{"chapter":70,"verse":26,"text":"والذين يصدقون بيوم الدين"},{"chapter":70,"verse":27,"text":"والذين هم من عذاب ربهم مشفقون"},{"chapter":70,"verse":28,"text":"ان عذاب ربهم غير مامون"},{"chapter":70,"verse":29,"text":"والذين هم لفروجهم حفظون"},{"chapter":70,"verse":30,"text":"الا على ازوجهم او ما ملكت ايمنهم فانهم غير ملومين"},{"chapter":70,"verse":31,"text":"فمن ابتغى وراء ذلك فاوليك هم العادون"},{"chapter":70,"verse":32,"text":"والذين هم لامنتهم وعهدهم رعون"},{"chapter":70,"verse":33,"text":"والذين هم بشهدتهم قايمون"},{"chapter":70,"verse":34,"text":"والذين هم على صلاتهم يحافظون"},{"chapter":70,"verse":35,"text":"اوليك في جنت مكرمون"},{"chapter":70,"verse":36,"text":"فمال الذين كفروا قبلك مهطعين"},{"chapter":70,"verse":37,"text":"عن اليمين وعن الشمال عزين"},{"chapter":70,"verse":38,"text":"ايطمع كل امري منهم ان يدخل جنة نعيم"},{"chapter":70,"verse":39,"text":"كلا انا خلقنهم مما يعلمون"}]}