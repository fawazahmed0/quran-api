rse":37,"text":"الم يك نطفة من مني يمنى"},{"chapter":75,"verse":38,"text":"ثم كان علقة فخلق فسوى"},{"chapter":75,"verse":39,"text":"فجعل منه الزوجين الذكر والانثى"},{"chapter":75,"verse":40,"text":"اليس ذلك بقدر على ان يحي الموتى"},{"chapter":76,"verse":1,"text":"هل اتى على الانسن حين من الدهر لم يكن شي ا مذكورا"},{"chapter":76,"verse":2,"text":"انا خلقنا الانسن من نطفة امشاج نبتليه فجعلنه سميعا بصيرا"},{"chapter":76,"verse":3,"text":"انا هدينه السبيل اما شاكر ا واما كفورا"},{"chapter":76,"verse":4,"text":"انا اعتدنا للكفرين سلسلا واغلل ا وسعيرا"},{"chapter":76,"verse":5,"text":"ان الابرار يشربون من كاس كان مزاجها كافورا"},{"chapter":76,"verse":6,"text":"عين ا يشرب بها عباد الله يفجرونها تفجير ا"},{"chapter":76,"verse":7,"text":"يوفون بالنذر ويخافون يوم ا كان شره مستطير ا"},{"chapter":76,"verse":8,"text":"ويطعمون الطعام على حبه مسكين ا ويتيم ا واسيرا"},{"chapter":76,"verse":9,"text":"انما نطعمكم لوجه الله لا نريد منكم جزاء ولا شكورا"},{"chapter":76,"verse":10,"text":"انا نخاف من ربنا يوما عبوس ا قمطرير ا"},{"chapter":76,"verse":11,"text":"فوقىهم الله شر ذلك اليوم ولقىهم نضرة وسرور ا"},{"chapter":76,"verse":12,"text":"وجزىهم بما صبروا جنة وحرير ا"},{"chapter":76,"verse":13,"text":"متكين فيها على الارايك لا يرون فيها شمس ا ولا زمهرير ا"},{"chapter":76,"verse":14,"text":"ودانية عليهم ظللها وذللت قطوفها تذليل ا"},{"chapter":76,"verse":15,"text":"ويطاف عليهم بانية من فضة واكواب كانت قواريرا"},{"chapter":76,"verse":16,"text":"قواريرا من فضة قدروها تقدير ا"},{"chapter":76,"verse":17,"text":"ويسقون فيها كاس ا كان مزاجها زنجبيلا"},{"chapter":76,"verse":18,"text":"عين ا فيها تسمى سلسبيل ا"}]}