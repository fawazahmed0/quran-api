{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Znas li ti onoga koji Sud porice"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroce"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah - ne podstice"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa tesko klanjacima"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onima koji su nemarni prema svome namazu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo ljudima pokazuju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "cak potrepstine koje se na posudbu daju oni ne daju"
        }
    ]
}