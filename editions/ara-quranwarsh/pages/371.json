{
    "pages": [
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاجْعَل لِّے لِسَانَ صِدْق ࣲ فِے اِ۬لَاخِرِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاجْعَلْنِے مِنْ وَّرَثَةِ جَنَّةِ اِ۬لنَّعِيمِۖ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاغْفِرْ لِأَبِيَ إِنَّهُۥ كَانَ مِنَ اَ۬لضَّآلِّينَۖ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخْزِنِے يَوْمَ يُبْعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوْمَ لَا يَنفَعُ مَال ࣱ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنَ اَتَي اَ۬للَّهَ بِقَلْب ࣲ سَلِيم ࣲ ۖ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزْلِفَتِ اِ۬لْجَنَّةُ لِلْمُتَّقِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ اِ۬لْجَحِيمُ لِلْغَاوِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيلَ لَهُمُۥٓ أَيْنَ مَا كُنتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِن دُونِ اِ۬للَّهِۖ هَلْ يَنصُرُونَكُمُۥٓ أَوْ يَنتَصِرُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبْكِبُواْ فِيهَا هُمْ وَالْغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُواْ وَهُمْ فِيهَا يَخْتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاللَّهِ إِن كُنَّا لَفِے ضَلَٰل ࣲ مُّبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "اِذْ نُسَوِّيكُم بِرَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَآ أَضَلَّنَآ إِلَّا اَ۬لْمُجْرِمُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِن شَٰفِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيقٍ حَمِيم ࣲ ۖ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوَ اَنَّ لَنَا كَرَّة ࣰ فَنَكُونَ مِنَ اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتْ قَوْمُ نُوحٍ اِ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ نُوحٌ اَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّے لَكُمْ رَسُولٌ اَمِين ࣱ ۖ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَيٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قَالُوٓاْ أَنُومِنُ لَكَ وَاتَّبَعَكَ اَ۬لَارْذَلُونَۖ"
        }
    ]
}