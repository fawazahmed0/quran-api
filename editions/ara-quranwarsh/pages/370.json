{
    "pages": [
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا اَ۬لْجَمْعَٰنِ قَالَ أَصْحَٰبُ مُوس۪يٰٓ إِنَّا لَمُدْرَكُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّآۖ إِنَّ مَعِے رَبِّے سَيَهْدِينِۖ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوْحَيْنَآ إِلَيٰ مُوس۪يٰٓ أَنِ اِ۪ضْرِب بِّعَصَاكَ اَ۬لْبَحْرَ فَانفَلَقَ فَكَانَ كُلُّ فِرْق ࣲ كَالطَّوْدِ اِ۬لْعَظِيمِۖ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزْلَفْنَا ثَمَّ اَ۬لَاخَرِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيْنَا مُوس۪يٰ وَمَن مَّعَهُۥٓ أَجْمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغْرَقْنَا اَ۬لَاخَرِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاتْلُ عَلَيْهِمْ نَبَأَ ا۪بْرَٰهِيمَ"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا تَعْبُدُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُواْ نَعْبُدُ أَصْنَاما ࣰ فَنَظَلُّ لَهَا عَٰكِفِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلْ يَسْمَعُونَكُمُۥٓ إِذْ تَدْعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوْ يَنفَعُونَكُمُۥٓ أَوْ يَضُرُّونَۖ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُواْ بَلْ وَجَدْنَآ ءَابَآءَنَا كَذَٰلِكَ يَفْعَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَٰٓيْتُم مَّا كُنتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمْ وَءَابَآؤُكُمُ اُ۬لَاقْدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمْ عَدُوّ ࣱ لِّيَۖ إِلَّا رَبَّ اَ۬لْعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "اَ۬لذِے خَلَقَنِے فَهُوَ يَهْدِينِۖ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَالذِے هُوَ يُطْعِمُنِے وَيَسْقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِۖ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَالذِے يُمِيتُنِے ثُمَّ يُحْيِينِۖ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "۞وَالذِےٓ أَطْمَعُ أَنْ يَّغْفِرَ لِے خَطِيَٓٔتِے يَوْمَ اَ۬لدِّينِۖ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبْ لِے حُكْما ࣰ وَأَلْحِقْنِے بِالصَّٰلِحِينَۖ"
        }
    ]
}