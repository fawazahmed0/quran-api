{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "وَالسَّمَآءِ وَالطَّارِقِ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لطَّارِقُ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "اُ۬لنَّجْمُ اُ۬لثَّاقِبُ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "إِن كُلُّ نَفْس ࣲ لَّمَا عَلَيْهَا حَافِظ ࣱ ۖ"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فَلْيَنظُرِ اِ۬لِانسَٰنُ مِمَّ خُلِقَۖ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خُلِقَ مِن مَّآء ࣲ دَافِق ࣲ‏"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "يَخْرُجُ مِنۢ بَيْنِ اِ۬لصُّلْبِ وَالتَّرَآئِبِۖ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "إِنَّهُۥ عَلَيٰ رَجْعِهِۦ لَقَادِر ࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "يَوْمَ تُبْلَي اَ۬لسَّرَآئِرُ"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فَمَا لَهُۥ مِن قُوَّة ࣲ وَلَا نَاصِر ࣲ ۖ"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "وَالسَّمَآءِ ذَاتِ اِ۬لرَّجْعِ"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "وَالَارْضِ ذَاتِ اِ۬لصَّدْعِ"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "إِنَّهُۥ لَقَوْل ࣱ فَصْل ࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وَمَا هُوَ بِالْهَزْلِۖ"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "إِنَّهُمْ يَكِيدُونَ كَيْدا ࣰ‏"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "وَأَكِيدُ كَيْدا ࣰ ۖ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فَمَهِّلِ اِ۬لْكٰ۪فِرِينَ أَمْهِلْهُمْ رُوَيْدا ࣰ ۖ"
        }
    ]
}