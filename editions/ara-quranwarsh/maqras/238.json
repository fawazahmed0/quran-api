{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "ۖ لَآ أُقْسِمُ بِهَٰذَا اَ۬لْبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا اَ۬لْبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِد ࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِے كَبَدٍۖ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "اَيَحْسِبُ أَن لَّنْ يَّقْدِرَ عَلَيْهِ أَحَد ࣱ‏"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُولُ أَهْلَكْتُ مَالا ࣰ لُّبَداًۖ"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "اَيَحْسِبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌۖ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "اَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانا ࣰ وَشَفَتَيْنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيْنَٰهُ اُ۬لنَّجْدَيْنِۖ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا اَ۪قْتَحَمَ اَ۬لْعَقَبَةَۖ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْعَقَبَةُۖ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكُّ رَقَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "اَوِ اِطْعَام ࣱ فِے يَوْم ࣲ ذِے مَسْغَبَة ࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَتِيما ࣰ ذَا مَقْرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "اَوْ مِسْكِينا ࣰ ذَا مَتْرَبَة ࣲ ۖ"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ اَ۬لذِينَ ءَامَنُواْ وَتَوَاصَوْاْ بِالصَّبْرِ وَتَوَاصَوْاْ بِالْمَرْحَمَةِۖ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُوْلَٰٓئِكَ أَصْحَٰبُ اُ۬لْمَيْمَنَةِۖ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَالذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمُۥٓ أَصْحَٰبُ اُ۬لْمَشْـَٔمَةِۖ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيْهِمْ نَار ࣱ مُّوصَدَة ࣱ ۖ"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَالشَّمْسِ وَضُحَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَالْقَمَرِ إِذَا تَلَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَالنَّه۪ارِ إِذَا جَلَّيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَاليْلِ إِذَا يَغْشَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَالسَّمَآءِ وَمَا بَنَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَالَارْضِ وَمَا طَحَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفْس ࣲ وَمَا سَوَّيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدَ اَفْلَحَ مَن زَكَّيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدْ خَابَ مَن دَسَّيٰهَاۖ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَتْ ثَمُودُ بِطَغْوَيٰهَآ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "إِذِ اِ۪نۢبَعَثَ أَشْقَيٰهَا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمْ رَسُولُ اُ۬للَّهِ نَاقَةَ اَ۬للَّهِ وَسُقْيَاهَا"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّيٰهَاۖ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "فَلَا يَخَافُ عُقْبَٰهَاۖ"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَاليْلِ إِذَا يَغْش۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَالنَّه۪ارِ إِذَا تَجَلّ۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ اَ۬لذَّكَرَ وَالُانث۪يٰٓ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعْيَكُمْ لَشَتّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنَ اَعْط۪يٰ وَاتَّق۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِالْحُسْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلْيُسْر۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَاسْتَغْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِالْحُسْن۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلْعُسْر۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغْنِے عَنْهُ مَالُهُۥٓ إِذَا تَرَدّ۪يٰٓۖ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَيْنَا لَلْهُد۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلَاخِرَةَ وَالُاول۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرْتُكُمْ نَارا ࣰ تَلَظّ۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصْلَيٰهَآ إِلَّا اَ۬لَاشْقَي"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "اَ۬لذِے كَذَّبَ وَتَوَلّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا اَ۬لَاتْقَي"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "اَ۬لذِے يُوتِے مَالَهُۥ يَتَزَكّ۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَة ࣲ تُجْز۪يٰٓ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا اَ۪بْتِغَآءَ وَجْهِ رَبِّهِ اِ۬لَاعْل۪يٰۖ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوْفَ يَرْض۪يٰۖ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَالضُّح۪يٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَاليْلِ إِذَا سَج۪يٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَل۪يٰۖ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلَاخِرَةُ خَيْر ࣱ لَّكَ مِنَ اَ۬لُاول۪يٰۖ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْض۪يٰٓۖ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمْ يَجِدْكَ يَتِيما ࣰ فَـَٔاو۪يٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلّا ࣰ فَهَد۪يٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلا ࣰ فَأَغْن۪يٰۖ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا اَ۬لْيَتِيمَ فَلَا تَقْهَرْۖ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا اَ۬لسَّآئِلَ فَلَا تَنْهَرْۖ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثَۖ"
        }
    ]
}