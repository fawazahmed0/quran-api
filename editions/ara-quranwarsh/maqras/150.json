{
    "maqras": [
        {
            "chapter": 26,
            "verse": 181,
            "text": "۞أَوْفُواْ اُ۬لْكَيْلَۖ وَلَا تَكُونُواْ مِنَ اَ۬لْمُخْسِرِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُواْ بِالْقُسْطَاسِ اِ۬لْمُسْتَقِيمِۖ"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبْخَسُواْ اُ۬لنَّاسَ أَشْيَآءَهُمْۖ وَلَا تَعْثَوْاْ فِے اِ۬لَارْضِ مُفْسِدِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاتَّقُواْ اُ۬لذِے خَلَقَكُمْ وَالْجِبِلَّةَ اَ۬لَاوَّلِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ اَ۬لْمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَآ أَنتَ إِلَّا بَشَر ࣱ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ اَ۬لْكَٰذِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَأَسْقِطْ عَلَيْنَا كِسْفا ࣰ مِّنَ اَ۬لسَّمَآءِ ان كُنتَ مِنَ اَ۬لصَّٰدِقِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيَ أَعْلَمُ بِمَا تَعْمَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ اِ۬لظُّلَّةِۖ إِنَّهُۥ كَانَ عَذَابَ يَوْمٍ عَظِيمٍۖ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "اِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ اِ۬لرُّوحُ اُ۬لَامِينُ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلَيٰ قَلْبِكَ لِتَكُونَ مِنَ اَ۬لْمُنذِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيّ ࣲ مُّبِين ࣲ ۖ"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَإِنَّهُۥ لَفِے زُبُرِ اِ۬لَاوَّلِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "أَوَلَمْ يَكُن لَّهُمُۥٓ ءَايَةً اَنْ يَّعْلَمَهُۥ عُلَمَٰٓؤُاْ بَنِےٓ إِسْرَآءِيلَۖ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوْ نَزَّلْنَٰهُ عَلَيٰ بَعْضِ اِ۬لَاعْجَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَأَهُۥ عَلَيْهِم مَّا كَانُواْ بِهِۦ مُومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذَٰلِكَ سَلَكْنَٰهُ فِے قُلُوبِ اِ۬لْمُجْرِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُومِنُونَ بِهِۦ حَتَّيٰ يَرَوُاْ اُ۬لْعَذَابَ اَ۬لَالِيمَ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَاتِيَهُم بَغْتَة ࣰ وَهُمْ لَا يَشْعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُولُواْ هَلْ نَحْنُ مُنظَرُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "أَفَرَٰٓيْتَ إِن مَّتَّعْنَٰهُمْ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُم مَّا كَانُواْ يُوعَدُونَ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَآ أَغْن۪يٰ عَنْهُم مَّا كَانُواْ يُمَتَّعُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ اِلَّا لَهَا مُنذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكْر۪يٰۖ وَمَا كُنَّا ظَٰلِمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "۞وَمَا تَنَزَّلَتْ بِهِ اِ۬لشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنۢبَغِے لَهُمْ وَمَا يَسْتَطِيعُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "إِنَّهُمْ عَنِ اِ۬لسَّمْعِ لَمَعْزُولُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدْعُ مَعَ اَ۬للَّهِ إِلَٰهاً اٰخَرَ فَتَكُونَ مِنَ اَ۬لْمُعَذَّبِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَأَنذِرْ عَشِيرَتَكَ اَ۬لَاقْرَبِينَ"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اِ۪تَّبَعَكَ مِنَ اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَإِنْ عَصَوْكَ فَقُلِ اِنِّے بَرِےٓء ࣱ مِّمَّا تَعْمَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "فَتَوَكَّلْ عَلَي اَ۬لْعَزِيزِ اِ۬لرَّحِيمِ"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "اِ۬لذِے يَر۪يٰكَ حِينَ تَقُومُ"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِے اِ۬لسَّٰجِدِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "إِنَّهُۥ هُوَ اَ۬لسَّمِيعُ اُ۬لْعَلِيمُۖ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلُ ا۟نَبِّئُكُمْ عَلَيٰ مَن تَنَزَّلُ اُ۬لشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلَيٰ كُلِّ أَفَّاكٍ اَثِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُلْقُونَ اَ۬لسَّمْعَ وَأَكْثَرُهُمْ كَٰذِبُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَالشُّعَرَآءُ يَتْبَعُهُمُ اُ۬لْغَاوُۥنَۖ"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "أَلَمْ تَرَ أَنَّهُمْ فِے كُلِّ وَاد ࣲ يَهِيمُونَ"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "إِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَذَكَرُواْ اُ۬للَّهَ كَثِيرا ࣰ وَانتَصَرُواْ مِنۢ بَعْدِ مَا ظُلِمُواْۖ وَسَيَعْلَمُ اُ۬لذِينَ ظَلَمُوٓاْ أَيَّ مُنقَلَب ࣲ يَنقَلِبُونَۖ"
        }
    ]
}