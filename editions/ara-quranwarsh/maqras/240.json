{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞اَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِے اِ۬لْقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِے اِ۬لصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذ ࣲ لَّخَبِيرٌۖ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "اِ۬لْقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "مَا اَ۬لْقَارِعَةُۖ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْقَارِعَةُۖ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يَوْمَ يَكُونُ اُ۬لنَّاسُ كَالْفَرَاشِ اِ۬لْمَبْثُوثِ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وَتَكُونُ اُ۬لْجِبَالُ كَالْعِهْنِ اِ۬لْمَنفُوشِۖ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فَهُوَ فِے عِيشَة ࣲ رَّاضِيَة ࣲ ۖ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فَأُمُّهُۥ هَاوِيَة ࣱ ۖ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وَمَآ أَدْر۪يٰكَ مَا هِيَهْۖ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نَارٌ حَامِيَةٌۖ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "اَلْه۪يٰكُمُ اُ۬لتَّكَاثُرُ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حَتَّيٰ زُرْتُمُ اُ۬لْمَقَابِرَۖ"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كَلَّا سَوْفَ تَعْلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَۖ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ اَ۬لْيَقِينِۖ"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لَتَرَوُنَّ اَ۬لْجَحِيمَ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ اَ۬لْيَقِينِ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ اِ۬لنَّعِيمِۖ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَالْعَصْرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ اَ۬لِانسَٰنَ لَفِے خُسْرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "اِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَتَوَاصَوْاْ بِالْحَقِّ وَتَوَاصَوْاْ بِالصَّبْرِۖ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "ۖ وَيْل ࣱ لِّكُلِّ هُمَزَة ࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "اِ۬لذِے جَمَعَ مَالا ࣰ وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحْسِبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِے اِ۬لْحُطَمَةِۖ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْحُطَمَةُۖ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ اُ۬للَّهِ اِ۬لْمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "اُ۬لتِے تَطَّلِعُ عَلَي اَ۬لَافْـِٕدَةِۖ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيْهِم مُّوصَدَة ࣱ‏"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِے عَمَد ࣲ مُّمَدَّدَةٍۖ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ اِ۬لْفِيلِۖ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِے تَضْلِيل ࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرْسَلَ عَلَيْهِمْ طَيْراً اَبَابِيلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرْمِيهِم بِحِجَارَة ࣲ مِّن سِجِّيل ࣲ‏"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمْ كَعَصْف ࣲ مَّاكُول ࣲ ۖ"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لِّإِيلَٰفِ قُرَيْشٍ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "اِيلَٰفِهِمْ رِحْلَةَ اَ۬لشِّتَآءِ وَالصَّيْفِۖ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَلْيَعْبُدُواْ رَبَّ هَٰذَا اَ۬لْبَيْتِ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "اِ۬لذِےٓ أَطْعَمَهُم مِّن جُوع ࣲ وَءَامَنَهُم مِّنْ خَوْفٍۖ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "اَرَٰٓيْتَ اَ۬لذِے يُكَذِّبُ بِالدِّينِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَٰلِكَ اَ۬لذِے يَدُعُّ اُ۬لْيَتِيمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا يَحُضُّ عَلَيٰ طَعَامِ اِ۬لْمِسْكِينِۖ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَيْل ࣱ لِّلْمُصَلِّينَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "اَ۬لذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "اَ۬لذِينَ هُمْ يُرَآءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَيَمْنَعُونَ اَ۬لْمَاعُونَۖ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "إِنَّآ أَعْطَيْنَٰكَ اَ۬لْكَوْثَرَ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَانْحَرِۖ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "اِنَّ شَانِئَكَ هُوَ اَ۬لَابْتَرُۖ"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قُلْ يَٰٓأَيُّهَا اَ۬لْكَٰفِرُونَ"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لَآ أَعْبُدُ مَا تَعْبُدُونَ"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُۖ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "وَلَآ أَنَا عَابِد ࣱ مَّا عَبَدتُّمْ"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُۖ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لَكُمْ دِينُكُمْ وَلِيَ دِينِۖ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "إِذَا جَآءَ نَصْرُ اُ۬للَّهِ وَالْفَتْحُ"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "وَرَأَيْتَ اَ۬لنَّاسَ يَدْخُلُونَ فِے دِينِ اِ۬للَّهِ أَفْوَاجا ࣰ‏"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فَسَبِّحْ بِحَمْدِ رَبِّكَ وَاسْتَغْفِرْهُۖ إِنَّهُۥ كَانَ تَوَّابا ࣰ ۖ"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتْ يَدَآ أَبِے لَهَب ࣲ وَتَبَّۖ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَآ أَغْن۪يٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَۖ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَيَصْلَيٰ نَارا ࣰ ذَاتَ لَهَب ࣲ‏"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَامْرَأَتُهُۥۖ حَمَّالَةُ اُ۬لْحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِے جِيدِهَا حَبْل ࣱ مِّن مَّسَد ࣲ ۖ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قُلْ هُوَ اَ۬للَّهُ أَحَدٌۖ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اِ۬للَّهُ اُ۬لصَّمَدُۖ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لَمْ يَلِدْ وَلَمْ يُولَدْۖ"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "وَلَمْ يَكُن لَّهُۥ كُفُؤاً اَحَد ࣱ ۖ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلَ اَعُوذُ بِرَبِّ اِ۬لْفَلَقِ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِن شَرِّ مَا خَلَقَ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِن شَرِّ غَاسِقٍ اِذَا وَقَبَ"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِن شَرِّ اِ۬لنَّفَّٰثَٰتِ فِے اِ۬لْعُقَدِ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِن شَرِّ حَاسِدٍ اِذَا حَسَدَۖ"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قُلَ اَعُوذُ بِرَبِّ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "إِلَٰهِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِن شَرِّ اِ۬لْوَسْوَاسِ اِ۬لْخَنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "اِ۬لذِے يُوَسْوِسُ فِے صُدُورِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ اَ۬لْجِنَّةِ وَالنَّاسِۖ"
        }
    ]
}