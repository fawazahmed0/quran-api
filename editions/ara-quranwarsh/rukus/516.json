{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "وَالْمُرْسَلَٰتِ عُرْفا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فَالْعَٰصِفَٰتِ عَصْفا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "وَالنَّٰشِرَٰتِ نَشْرا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فَالْفَٰرِقَٰتِ فَرْقا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فَالْمُلْقِيَٰتِ ذِكْراً"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عُذْراً اَوْ نُذُراً"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "اِنَّمَا تُوعَدُونَ لَوَٰقِع ࣱ ۖ"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فَإِذَا اَ۬لنُّجُومُ طُمِسَتْ"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "وَإِذَا اَ۬لسَّمَآءُ فُرِجَتْ"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "وَإِذَا اَ۬لْجِبَالُ نُسِفَتْ"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "وَإِذَا اَ۬لرُّسُلُ أُقِّتَتْ"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لِأَيِّ يَوْمٍ ا۟جِّلَتْ"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "لِيَوْمِ اِ۬لْفَصْلِۖ"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وَمَآ أَدْر۪يٰكَ مَا يَوْمُ اُ۬لْفَصْلِۖ"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "۞أَلَمْ نُهْلِكِ اِ۬لَاوَّلِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثُمَّ نُتْبِعُهُمُ اُ۬لَاخِرِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "أَلَمْ نَخْلُقكُّم مِّن مَّآء ࣲ مَّهِين ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فَجَعَلْنَٰهُ فِے قَر۪ار ࣲ مَّكِينٍ"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "اِلَيٰ قَدَر ࣲ مَّعْلُوم ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فَقَدَّرْنَاۖ فَنِعْمَ اَ۬لْقَٰدِرُونَۖ"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "أَلَمْ نَجْعَلِ اِ۬لَارْضَ كِفَاتاً"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "اَحْيَآء ࣰ وَأَمْوَٰتا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَجَعَلْنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰت ࣲ وَأَسْقَيْنَٰكُم مَّآء ࣰ فُرَاتا ࣰ ۖ"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "اَ۪نطَلِقُوٓاْ إِلَيٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "اَ۪نطَلِقُوٓاْ إِلَيٰ ظِلّ ࣲ ذِے ثَلَٰثِ شُعَب ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيل ࣲ وَلَا يُغْنِے مِنَ اَ۬للَّهَبِۖ"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "إِنَّهَا تَرْمِے بِشَرَر ࣲ كَالْقَصْرِ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَأَنَّهُۥ جِمَٰلَٰت ࣱ صُفْر ࣱ ۖ"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هَٰذَا يَوْمُ لَا يَنطِقُونَ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُوذَنُ لَهُمْ فَيَعْتَذِرُونَۖ"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هَٰذَا يَوْمُ اُ۬لْفَصْلِ جَمَعْنَٰكُمْ وَالَاوَّلِينَ"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَإِن كَانَ لَكُمْ كَيْد ࣱ فَكِيدُونِۖ"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَۖ"
        }
    ]
}