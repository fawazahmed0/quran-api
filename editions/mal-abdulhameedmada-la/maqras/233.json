{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "entinepparriyan avar parasparam ceadiccu keantirikkunnat‌"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "a mahattaya vrttantattepparri"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "avar etearu karyattil abhiprayavyatyasattilayi kkeantirikkunnuvea atinepparri"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "nis'sansayam; avar valiye arinnu keallum"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "vintum nis'sansayam; avar valiye arinnu keallum"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "bhumiye nam oru virippakkiyille"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "parvvatannale anikalakkukayum (ceytille)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "ninnale nam inakalayi srstikkukayum ceytirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "ninnalute urakkatte nam visramamakkukayum ceytirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "ratriye nam oru vastramakkukayum"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "pakaline nam jivasandharanavelayakkukayum ceytirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "ninnalkk mite balisthamaya elu akasannal nam nirmikkukayum"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "kattijvalikkunna oru vilakk nam untakkukayum ceytirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "karmeghannalil ninn kutti olukunna vellam nam irakkukayum ceytu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "atu mukhena dhan'yavum sasyavum nam purattu keantu varan venti"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "itaturnna teattannalum"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "tirccayayum tirumanattinre divasam samayam nirnayikkappettatayirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "atayat kahalattil utappetukayum, ninnal kuttankuttamayi vannettukayum ceyyunna divasam"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "akasam turakkappetukayum ennittat pala kavatannalayi tirukayum ceyyum"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "parvvatannal sancarippikkappetukayum annane ava maricika peale ayittirukayum ceyyum"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "tirccayayum narakam kattirikkunna sthalamakunnu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "atikramakarikalkk matanniccellanulla sthalam"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "avar atil yugannalealam tamasikkunnavarayirikkum"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "kulirmayea kutinirea avar avite asvadikkukayilla"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "keatuncutulla vellavum keatum tanuppulla vellavumallate"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "anuyeajyamaya pratiphalamatre at‌"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "tirccayayum avar vicarana pratiksikkunnillayirunnu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "nam'mute drstantannale avar tirttum nisedhiccu tallukayum ceytu"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "etu karyavum nam eluti tittappetuttiyirikkunnu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "atinal ninnal (siksa) asvadiccu kealluka. tirccayayum nam ninnalkku siksayallateannum vard'dhippiccu tarikayilla"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "tirccayayum suksmata paliccavarkk vijayamunt‌"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "atayat (svargattile) teattannalum muntirikalum"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "tututta marvitamulla samaprayakkaraya tarunikalum"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "niranna panapatrannalum"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "avite anavasyamaya oru vakkea oru vyajavarttayea avar kelkkukayilla"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "(at‌) ninre raksitavinkal ninnulla oru pratiphalavum, kanakkeatta oru sam'manavumakunnu"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "akasannaluteyum bhumiyuteyum avakkitayilullatinreyum raksitavum paramakarunikanumayullavanre (sam'manam.) avanumayi sambhasanattil erpetan avarkku sadhikkukayilla"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "ruhum malakkukalum aniyayi nilkkunna divasam. paramakarunikanaya allahu anuvadam nalkiyittullavanum satyam parannittullavanumallate ann sansarikkukayilla"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "atatre yathart'thamaya divasam. atinal vallavanum uddesikkunna paksam tanre raksitavinkalekkulla matakkattinre margam avan svikarikkatte"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "asannamaya oru siksaye parri tirccayayum ninnalkku nam munnariyipp nalkiyirikkunnu. manusyan tanre kaikal munkutti ceytu veccat neakkikkanukayum, ayyea nan mannayirunnenkil etra nannayirunnene enn satyanisedhi parayukayum ceyyunna divasam"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "(avisvasikalilekk‌) iranniccenn (avarute atmavukale) uriyetukkunnava tanneyana satyam"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "(satyavisvasikalute atmavukale) semyatayeate purattetukkunnava tanneyana, satyam"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "ukkeate oluki varunnava tanneyana, satyam"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "ennittu munneattu kuticcu peakunnava tanneyana, satyam"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "karyam niyantrikkunnavayum tanneyana, satyam"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "a natukkunna sambhavam natukkamuntakkunna divasam"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "atine tutarnn atinre pinnale marreannum"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "cila hrdayannal annu viraccu keantirikkum"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "avayute kannukal ann kilpeattu talnnirikkum"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "avar parayum: tirccayayum nam (nam'mute) munsthitiyilekk matakkappetunnavaranea"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "nam jirnicca ellukalayi kalinnalum (namukk matakkamea)"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "avar parayukayan‌: annaneyanenkil nastakaramaya oru tiriccuvaravayirikkum at‌"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "at oreyearu ghearasabdam matramayirikkum"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "appealata avar bhumukhattettikkalinnu"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "musanabiyute varttamanam ninakk vannettiyea"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "tvuva enna parisud'dha tal‌varayil vecc addehattinre raksitav addehatte vilicc iprakaram paranna sandarbham"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ni phir'aunre atuttekku peakuka. tirccayayum avan atirukavinnirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "ennitt ceadikkuka: ni parisud'dhi prapikkan tayyaruntea"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "ninre raksitavinkalekk ninakk nan vali kaniccutaram. ennitt ni bhayappetanum (tayyaruntea)"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "annane addeham (musa) avann a mahattaya drstantam kaniccukeatuttu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "appeal avan nisedhiccu tallukayum dhikkarikkukayum ceytu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "pinne, avan etir sramannal natattuvanayi pintirinnu peayi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "annane avan (tanre alkkare) sekhariccu. ennittu vilambaram ceytu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "nan ninnalute atyunnatanaya raksitavakunnu enn avan parannu"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "appeal paraleakattileyum ihaleakattileyum siksaykkayi allahu avane pitikuti"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "tirccayayum atil bhayappetunnavarkk oru gunapathamunt‌"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ninnalanea srstikkappetan kututal prayasamullavar. atalla; akasamanea? atine avan nirmiccirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "atinre vitanam avan uyarttukayum, atine avan vyavasthappetuttukayum ceytirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "atile ratriye avan iruttakkukayum, atile pakaline avan pratyaksappetuttukayum ceytirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "atinu sesam bhumiye avan vikasippiccirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "atil ninn atile vellavum sasyajalannalum avan purattu keantuvarikayum ceytirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "parvvatannale avan urappiccu nirttukayum ceytirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "ninnalkkum ninnalute kannukalikalkkum upayeagattinayitt‌"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "ennal a maha vipatt varunna sandarbham"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "atayatu manusyan tan ad'dhvaniccu veccatinepparri ormikkunna divasam"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "kanunnavarkk venti narakam velivakkappetunna divasam"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "(ann‌) ar atirukaviyukayum"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "ihaleakajivitattinu kututal pradhan'yam nalkukayum ceytuvea"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "(avann‌) kattijvalikkunna narakam tanneyan sanketam"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "appeal etearal tanre raksitavinre sthanatte bhayappetukayum manas'sine tannistattil ninn vilakkinirttukayum ceytuvea"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "(avann‌) svargam tanneyan sanketam"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "a antyasamayattepparri, ateppealan sambhavikkuka enn avar ninneat ceadikkunnu"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "ninakk atinepparri ent parayananullat‌"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ninre raksitavinkalekkan atinre kalasam"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "atine bhayappetunnavarkk oru takkitukaran matraman ni"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "atine avar kanunna divasam oru vaikunneramea oru prabhatattilea allate avar (ivite) kaliccukuttiyittillatta pealeyayirikkum (avarkk teannuka)"
        }
    ]
}