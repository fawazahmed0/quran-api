{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "addeham mukham culiccu tirinnukalannu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "addehattinre (nabiyute) atutt a andhan vannatinal"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "(nabiye,) ninakk entariyam? ayal (andhan) oru vela parisud'dhi prapiccekkamallea"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "allenkil upadesam svikarikkukayum, a upadesam ayalkk prayeajanappetukayum ceytekkamallea"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "ennal svayam paryaptata naticcavanakatte"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ni avanre nere srad'dhatirikkunnu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "avan parisud'dhi prapikkatirikkunnatinal ninakkentan kurram"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "ennal ninre atukkal otivannavanakatte"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "(allahuve) avan bhayappetunnavanayikkeant‌"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "avanre karyattil ni asrad'dhakanikkunnu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "nis'sansayam it (khur'an) oru ulbeadhanamakunnu; tircca"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "atinal ar uddesikkunnuvea avanat ormicc keallatte"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "adaraniyamaya cila etukalilanat‌"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "aunnatyam nalkappettatum parisud'dhamakkappettatumaya (etukalil)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "cila sandesavahakarute kaikalilanat‌"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "man'yanmarum punyavanmarum ayittullavarute"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "manusyan nasamatayatte. entanavan itra nandikettavanakan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "etearu vastuvil ninnan allahu avane srsticcat‌"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "oru bijattil ninn avane srstikkukayum, ennitt avane (avanre karyam) vyavasthappetuttukayum ceytu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "pinnit avan margam eluppamakkukayum ceytu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "anantaram avane marippikkukayum, khab‌ril maraykkukayum ceytu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pinnit avan uddesikkumpeal avane uyirttelunnelpikkunnatan‌"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "nis'sansayam, avaneat allahu kalpiccat avan nirvahiccilla"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "ennal manusyan tanre bhaksanattepparri onnu cinticc neakkatte"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "nam saktiyayi mala vellam cearinnukeatuttu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pinnit nam bhumiye oru tarattil pilartti"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ennitt atil nam dhan'yam mulappiccu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "muntiriyum paccakkarikalum"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "olivum intappanayum"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "itaturnnu nilkkunna teattannalum"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "palavargavum pullum"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "ninnalkkum ninnalute kannukalikalkkum upayeagattinayitt‌"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "ennal cekitatappikkunna a sabdam vannal"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "atayat manusyan tanre saheadarane vitt otippeakunna divasam"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tanre matavineyum pitavineyum"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "bharyayeyum makkaleyum"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "avarilppetta orea manusyanum tanikk matiyavunnatra (cinta) visayam ann untayirikkum"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "ann cila mukhannal prasannatayullavayayirikkum"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "cirikkunnavayum santeasam keallunnavayum"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "vere cila mukhannalakatte ann peati purantirikkum"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "avaye kurirutt mutiyirikkum"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "akkuttarakunnu avisvasikalum adharm'makarikalumayittullavar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "suryan currippeatiyappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "naksatrannal utirnnu vilumpeal"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvvatannal sancarippikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "purnnagarbhinikalaya ottakannal avaganikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "van'yamrgannal orumiccukuttappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "samudrannal alikkattikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "atmavukal kuttiyinakkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "(jivaneate) kuliccu mutappetta penkuttiyeatu ceadikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "tan entearu kurrattinan keallappettat enn‌"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "(karm'mannal rekhappetuttiya) etukal turannuvekkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "uparileakam mara nikkikanikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "jvalikkunna narakagni alikkattikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargam atuttu keantuvarappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "orea vyaktiyum tan tayyarakkikkeantu vannittullat entenn ariyunnatan‌"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "pinvannippeakunnavaye (naksatrannale) kkeant nan satyam ceytu parayunnu"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "sancariccukeantirikkunnavayum apratyaksamayikkeantirikkunnavayum"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "ratri ninnumpeal atu keantum"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "prabhatam vitarnn varumpeal atu keantum (nan satyam ceytu parayunnu)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "tirccayayum it (khur'an) man'yanaya oru dutanre vakkakunnu"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "saktiyullavanum, sinhasanasthanaya allahuvinkal sthanamullavanumaya (dutanre)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avite anusarikkappetunnavanum visvastanumaya (dutanre)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "ninnalute kuttukaran (pravacakan) oru bhrantanalla tanne"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "tirccayayum addehatte (jib‌ril enna dutane) pratyaksamaya mandalattil vecc addeham kantittunt‌"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "addeham adrsyavarttayute karyattil pisukk kanikkunnavanumalla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "it (khur'an) sapikkappetta oru pisacinre vakkumalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "appeal enneattan ninnal peakunnat‌"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "it leakarkk ventiyulla oru ulbeadhanamallate marreannumalla"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "atayat ninnalute kuttattil ninn nere nilakeallan uddesiccavarkk venti"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "leakaraksitavaya allahu uddesikkunnuvenkilallate ninnal uddesikkukayilla"
        }
    ]
}