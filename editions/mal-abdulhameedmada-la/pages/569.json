{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "avarkk an'yean'yam kanikkappetum. tanre makkale prayascittamayi nalki keant a divasatte siksayil ninn meacanam netan kalinnirunnenkil enn kurravali agrahikkum"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "tanre bharyayeyum saheadaraneyum"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "tanikk abhayam nalkiyirunna tanre bandhukkaleyum"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bhumiyilulla muluvan alukaleyum. ennitt atavane raksappetuttukayum ceytirunnenkil enn‌"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "sansayam venta, tirccayayum at alikkattunna narakamakunnu"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "talayute tealiyuriccu kalayunna narakagni"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "pinneakkam marukayum, tirinnukalayukayum ceytavare at ksanikkum"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "sekhariccu suksiccu veccavareyum"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "tirccayayum manusyan srstikkappettirikkunnat anne arram aksamanayikkeantan‌"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "atayat tinma badhiccal pearutiket kanikkunnavanayi keantum"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "nanma kaivannal tatannu vekkunnavanayikeantum"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "namaskarikkunnavarealike"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "atayat tannalute namaskarattil sthiramayi nisthayullavar"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "tannalute svattukkalil nirnitamaya avakasam nalkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "ceadiccu varunnavannum upajivanam tatayappettavannum"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "pratiphaladinattil visvasikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "tannalute raksitavinre siksayeparri bhayamullavarumealike"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "tirccayayum avarute raksitavinre siksa (varikayillenn‌) samadhanappetan parrattatakunnu"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "tannalute guhyavayavannal kattusuksikkunnavarum (olike)"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "tannalute bharyamaruteyea, valam kaikal utamappetuttiyavaruteyea karyattilealike. tirccayayum avar aksepamuktarakunnu"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "ennal atilappuram arenkilum agrahikkunna paksam attarakkar tanneyakunnu atirukaviyunnavar"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "tannale visvasiccelpicca karyannalum tannalute utampatikalum paliccu pearunnavarum"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "tannalute saksyannal muraprakaram nirvahikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "tannalute namaskarannal nisthayeate nirvahikkunnavarum (olike)"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "attarakkar svargatteappukalil adarikkappetunnavarakunnu"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "appeal satyanisedhikalkkentu parri! avar ninre nere kaluttu nitti vannitt‌"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "valatteattum itatteattum kuttannalayi citaripeakunnu"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "sukhanubhutiyute svargattil tan pravesippikkappetanamenn avaril orea manusyanum meahikkunnuntea"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "atu venta. tirccayayum avarkkariyavunnatil ninnan avare nam srsticcittullat‌"
        }
    ]
}