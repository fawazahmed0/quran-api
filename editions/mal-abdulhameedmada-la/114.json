    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "parayuka: manusyarute raksitavineat nan saranam tetunnu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manusyarute rajavineat‌"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manusyarute daivatteat‌"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "durbeadhanam natatti pinmarikkalayunnavarekkeantulla ketutiyil ninn‌"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "manusyarute hrdayannalil durbeadhanam natattunnavar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "manusyarilum jinnukalilum pettavar"
        }
    ]
}