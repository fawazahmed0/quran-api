{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "ravinetanneyana satyam ; at mutikeantirikkumpeal"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "pakaline tanneyana satyam ; at pratyaksappetumpeal"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "anineyum pennineyum srsticca ritiye tanneyana satyam"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "tirccayayum ninnalute parisramam vibhinna rupattilullatakunnu"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "ennal etearal danam nalkukayum, suksmata palikkukayum"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "erravum uttamamayatine satyappetuttukayum ceytuvea"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "avannu nam erravum eluppamayatilekk sekaryappetutti keatukkunnatan‌"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "ennal ar pisukku kanikkukayum, svayam paryaptata natikkukayum"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "erravum uttamamayatine nisedhiccu tallukayum ceytuvea"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "avannu nam erravum nerukkamullatilekk sekaryamearukkikeatukkunnatan‌"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avan nasattil patikkumpeal avanre dhanam avann prayeajanappetunnatalla"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "tirccayayum margadarsanam nam'mute badhyatayakunnu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "tirccayayum namukkullatakunnu paraleakavum ihaleakavum"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "atinal alikkattikkeantirikkunna agniyepparri nan ninnalkk takkit nalkiyirikkunnu"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "erravum dustanaya vyaktiyallate atil pravesikkukayilla"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "nisedhiccu tallukayum, pintirinnu kalayukayum (vyakti)"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "erravum suksmatayulla vyakti atil ninn akarri nirttappetunnatan‌"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "parisud'dhinetuvanayi tanre dhanam nalkunna (vyakti)"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "pratyupakaram nalkappetentataya yatearu anugrahavum avanre pakkal oralkkumilla"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "tanre atyunnatanaya raksitavinre priti tetuka ennatallate"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "valiye avan trptippetunnatan‌"
        }
    ]
}