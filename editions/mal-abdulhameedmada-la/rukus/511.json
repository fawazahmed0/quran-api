{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "nis'sansayam, candrane tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "ratri pinnitt peakumpeal atine tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "prabhatam pularnnal atu tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "tirccayayum at (narakam) geravamulla karyannalil onnakunnu"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "manusyarkk oru takkitenna nilayil"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "atayat ninnalil ninn munneatt peakuvanea, pinneatt vekkuvanea uddesikkunnavarkk‌"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "orea vyaktiyum tan sampadiccu veccatin panayappettavanakunnu"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "valatupaksakkarealike"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "cila svargatteappukalilayirikkum avar. avar anvesikkum"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "kurravalikalepparri"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ninnale narakattil pravesippiccat enteannanenn‌"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "avar (kurravalikal) marupati parayum: nannal namaskarikkunnavarute kuttattilayilla"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "nannal agatikk aharam nalkumayirunnilla"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "teannivasattil mulukunnavarute kute nannalum mulukumayirunnu"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "pratiphalattinre naline nannal nisedhiccu kalayumayirunnu"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "annaneyirikke a urappaya maranam nannalkk vannetti"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "ini avarkk suparsakkarute suparsayeannum prayeajanappetukayilla"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "ennirikke avarkkentu parri? avar ulbeadhanattil ninn tirinnukalayunnavarayirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "avar virali piticca kalutakaleppealirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "sinhattil ninn otiraksappetunna (kalutakal)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "alla, avaril orearuttarum agrahikkunnu; tanikk allahuvinkal ninn nivarttiya etukal nalkappetanamenn‌"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "alla; pakse, avar paraleakatte bhayappetunnilla"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "alla; tirccayayum it oru ulbeadhanamakunnu"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "akayal ar uddesikkunnuvea avarat ormiccu keallatte"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "allahu uddesikkunnuvenkilallate avar ormikkunnatalla. avanakunnu bhaktikkavakasappettavan; papameacanattin avakasappettavan"
        }
    ]
}