{"rukus":[{"chapter":114,"verse":1,"text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ النَّاسِ"},{"chapter":114,"verse":2,"text":"مَلِكِ النَّاسِ"},{"chapter":114,"verse":3,"text":"إِلَٰهِ النَّاسِ"},{"chapter":114,"verse":4,"text":"مِنْ شَرِّ الْوَسْوَاسِ الْخَنَّاسِ"},{"chapter":114,"verse":5,"text":"الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ"},{"chapter":114,"verse":6,"text":"مِنَ الْجِنَّةِ وَالنَّاسِ"}]}