{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ طسم"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "لَعَلَّكَ بَاخِعٌ نَفْسَكَ أَلَّا يَكُونُوا مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "إِنْ نَشَأْ نُنَزِّلْ عَلَيْهِمْ مِنَ السَّمَاءِ آيَةً فَظَلَّتْ أَعْنَاقُهُمْ لَهَا خَاضِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "وَمَا يَأْتِيهِمْ مِنْ ذِكْرٍ مِنَ الرَّحْمَٰنِ مُحْدَثٍ إِلَّا كَانُوا عَنْهُ مُعْرِضِينَ"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "فَقَدْ كَذَّبُوا فَسَيَأْتِيهِمْ أَنْبَاءُ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "أَوَلَمْ يَرَوْا إِلَى الْأَرْضِ كَمْ أَنْبَتْنَا فِيهَا مِنْ كُلِّ زَوْجٍ كَرِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰ أَنِ ائْتِ الْقَوْمَ الظَّالِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "قَالَ رَبِّ إِنِّي أَخَافُ أَنْ يُكَذِّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "وَيَضِيقُ صَدْرِي وَلَا يَنْطَلِقُ لِسَانِي فَأَرْسِلْ إِلَىٰ هَارُونَ"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "وَلَهُمْ عَلَيَّ ذَنْبٌ فَأَخَافُ أَنْ يَقْتُلُونِ"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "قَالَ كَلَّا ۖ فَاذْهَبَا بِآيَاتِنَا ۖ إِنَّا مَعَكُمْ مُسْتَمِعُونَ"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "فَأْتِيَا فِرْعَوْنَ فَقُولَا إِنَّا رَسُولُ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "أَنْ أَرْسِلْ مَعَنَا بَنِي إِسْرَائِيلَ"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "وَفَعَلْتَ فَعْلَتَكَ الَّتِي فَعَلْتَ وَأَنْتَ مِنَ الْكَافِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "قَالَ فَعَلْتُهَا إِذًا وَأَنَا مِنَ الضَّالِّينَ"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "فَفَرَرْتُ مِنْكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِي رَبِّي حُكْمًا وَجَعَلَنِي مِنَ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَيَّ أَنْ عَبَّدْتَ بَنِي إِسْرَائِيلَ"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "قَالَ فِرْعَوْنُ وَمَا رَبُّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "قَالَ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ إِنْ كُنْتُمْ مُوقِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "قَالَ لِمَنْ حَوْلَهُ أَلَا تَسْتَمِعُونَ"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "قَالَ رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "قَالَ إِنَّ رَسُولَكُمُ الَّذِي أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "قَالَ رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَيْنَهُمَا ۖ إِنْ كُنْتُمْ تَعْقِلُونَ"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "قَالَ لَئِنِ اتَّخَذْتَ إِلَٰهًا غَيْرِي لَأَجْعَلَنَّكَ مِنَ الْمَسْجُونِينَ"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "قَالَ أَوَلَوْ جِئْتُكَ بِشَيْءٍ مُبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "قَالَ فَأْتِ بِهِ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِيَ ثُعْبَانٌ مُبِينٌ"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "وَنَزَعَ يَدَهُ فَإِذَا هِيَ بَيْضَاءُ لِلنَّاظِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "قَالَ لِلْمَلَإِ حَوْلَهُ إِنَّ هَٰذَا لَسَاحِرٌ عَلِيمٌ"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "يُرِيدُ أَنْ يُخْرِجَكُمْ مِنْ أَرْضِكُمْ بِسِحْرِهِ فَمَاذَا تَأْمُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "قَالُوا أَرْجِهْ وَأَخَاهُ وَابْعَثْ فِي الْمَدَائِنِ حَاشِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "فَجُمِعَ السَّحَرَةُ لِمِيقَاتِ يَوْمٍ مَعْلُومٍ"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "وَقِيلَ لِلنَّاسِ هَلْ أَنْتُمْ مُجْتَمِعُونَ"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "لَعَلَّنَا نَتَّبِعُ السَّحَرَةَ إِنْ كَانُوا هُمُ الْغَالِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "فَلَمَّا جَاءَ السَّحَرَةُ قَالُوا لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِنْ كُنَّا نَحْنُ الْغَالِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَمِنَ الْمُقَرَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "قَالَ لَهُمْ مُوسَىٰ أَلْقُوا مَا أَنْتُمْ مُلْقُونَ"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "فَأَلْقَوْا حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ الْغَالِبُونَ"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِيَ تَلْقَفُ مَا يَأْفِكُونَ"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "فَأُلْقِيَ السَّحَرَةُ سَاجِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "قَالُوا آمَنَّا بِرَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "رَبِّ مُوسَىٰ وَهَارُونَ"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "قَالَ آمَنْتُمْ لَهُ قَبْلَ أَنْ آذَنَ لَكُمْ ۖ إِنَّهُ لَكَبِيرُكُمُ الَّذِي عَلَّمَكُمُ السِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُمْ مِنْ خِلَافٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "قَالُوا لَا ضَيْرَ ۖ إِنَّا إِلَىٰ رَبِّنَا مُنْقَلِبُونَ"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "إِنَّا نَطْمَعُ أَنْ يَغْفِرَ لَنَا رَبُّنَا خَطَايَانَا أَنْ كُنَّا أَوَّلَ الْمُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "۞ وَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ بِعِبَادِي إِنَّكُمْ مُتَّبَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَأَرْسَلَ فِرْعَوْنُ فِي الْمَدَائِنِ حَاشِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "إِنَّ هَٰؤُلَاءِ لَشِرْذِمَةٌ قَلِيلُونَ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَإِنَّهُمْ لَنَا لَغَائِظُونَ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَإِنَّا لَجَمِيعٌ حَاذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَأَخْرَجْنَاهُمْ مِنْ جَنَّاتٍ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذَٰلِكَ وَأَوْرَثْنَاهَا بَنِي إِسْرَائِيلَ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَأَتْبَعُوهُمْ مُشْرِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَاءَى الْجَمْعَانِ قَالَ أَصْحَابُ مُوسَىٰ إِنَّا لَمُدْرَكُونَ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّا ۖ إِنَّ مَعِيَ رَبِّي سَيَهْدِينِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنِ اضْرِبْ بِعَصَاكَ الْبَحْرَ ۖ فَانْفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَالطَّوْدِ الْعَظِيمِ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزْلَفْنَا ثَمَّ الْآخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنْجَيْنَا مُوسَىٰ وَمَنْ مَعَهُ أَجْمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاتْلُ عَلَيْهِمْ نَبَأَ إِبْرَاهِيمَ"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُوا نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَاكِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوْ يَنْفَعُونَكُمْ أَوْ يَضُرُّونَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُوا بَلْ وَجَدْنَا آبَاءَنَا كَذَٰلِكَ يَفْعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَأَيْتُمْ مَا كُنْتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنْتُمْ وَآبَاؤُكُمُ الْأَقْدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمْ عَدُوٌّ لِي إِلَّا رَبَّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "الَّذِي خَلَقَنِي فَهُوَ يَهْدِينِ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَالَّذِي هُوَ يُطْعِمُنِي وَيَسْقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَالَّذِي يُمِيتُنِي ثُمَّ يُحْيِينِ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَالَّذِي أَطْمَعُ أَنْ يَغْفِرَ لِي خَطِيئَتِي يَوْمَ الدِّينِ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبْ لِي حُكْمًا وَأَلْحِقْنِي بِالصَّالِحِينَ"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاجْعَلْ لِي لِسَانَ صِدْقٍ فِي الْآخِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاجْعَلْنِي مِنْ وَرَثَةِ جَنَّةِ النَّعِيمِ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاغْفِرْ لِأَبِي إِنَّهُ كَانَ مِنَ الضَّالِّينَ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخْزِنِي يَوْمَ يُبْعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوْمَ لَا يَنْفَعُ مَالٌ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنْ أَتَى اللَّهَ بِقَلْبٍ سَلِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِينَ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ الْجَحِيمُ لِلْغَاوِينَ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيلَ لَهُمْ أَيْنَ مَا كُنْتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِنْ دُونِ اللَّهِ هَلْ يَنْصُرُونَكُمْ أَوْ يَنْتَصِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبْكِبُوا فِيهَا هُمْ وَالْغَاوُونَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُوا وَهُمْ فِيهَا يَخْتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاللَّهِ إِنْ كُنَّا لَفِي ضَلَالٍ مُبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "إِذْ نُسَوِّيكُمْ بِرَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا أَضَلَّنَا إِلَّا الْمُجْرِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِنْ شَافِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيقٍ حَمِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوْ أَنَّ لَنَا كَرَّةً فَنَكُونَ مِنَ الْمُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتْ قَوْمُ نُوحٍ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذْ قَالَ لَهُمْ أَخُوهُمْ نُوحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞ قَالُوا أَنُؤْمِنُ لَكَ وَاتَّبَعَكَ الْأَرْذَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلْمِي بِمَا كَانُوا يَعْمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنْ حِسَابُهُمْ إِلَّا عَلَىٰ رَبِّي ۖ لَوْ تَشْعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَا أَنَا بِطَارِدِ الْمُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنْ أَنَا إِلَّا نَذِيرٌ مُبِينٌ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُوا لَئِنْ لَمْ تَنْتَهِ يَا نُوحُ لَتَكُونَنَّ مِنَ الْمَرْجُومِينَ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوْمِي كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَافْتَحْ بَيْنِي وَبَيْنَهُمْ فَتْحًا وَنَجِّنِي وَمَنْ مَعِيَ مِنَ الْمُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنْجَيْنَاهُ وَمَنْ مَعَهُ فِي الْفُلْكِ الْمَشْحُونِ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغْرَقْنَا بَعْدُ الْبَاقِينَ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتْ عَادٌ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذْ قَالَ لَهُمْ أَخُوهُمْ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبْنُونَ بِكُلِّ رِيعٍ آيَةً تَعْبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمْ تَخْلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشْتُمْ بَطَشْتُمْ جَبَّارِينَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَاتَّقُوا الَّذِي أَمَدَّكُمْ بِمَا تَعْلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُمْ بِأَنْعَامٍ وَبَنِينَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّاتٍ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّي أَخَافُ عَلَيْكُمْ عَذَابَ يَوْمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُوا سَوَاءٌ عَلَيْنَا أَوَعَظْتَ أَمْ لَمْ تَكُنْ مِنَ الْوَاعِظِينَ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنْ هَٰذَا إِلَّا خُلُقُ الْأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحْنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهْلَكْنَاهُمْ ۗ إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كَذَّبَتْ ثَمُودُ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "إِذْ قَالَ لَهُمْ أَخُوهُمْ صَالِحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "أَتُتْرَكُونَ فِي مَا هَاهُنَا آمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "فِي جَنَّاتٍ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وَزُرُوعٍ وَنَخْلٍ طَلْعُهَا هَضِيمٌ"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وَتَنْحِتُونَ مِنَ الْجِبَالِ بُيُوتًا فَارِهِينَ"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "وَلَا تُطِيعُوا أَمْرَ الْمُسْرِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "الَّذِينَ يُفْسِدُونَ فِي الْأَرْضِ وَلَا يُصْلِحُونَ"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قَالُوا إِنَّمَا أَنْتَ مِنَ الْمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "مَا أَنْتَ إِلَّا بَشَرٌ مِثْلُنَا فَأْتِ بِآيَةٍ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قَالَ هَٰذِهِ نَاقَةٌ لَهَا شِرْبٌ وَلَكُمْ شِرْبُ يَوْمٍ مَعْلُومٍ"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "وَلَا تَمَسُّوهَا بِسُوءٍ فَيَأْخُذَكُمْ عَذَابُ يَوْمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فَعَقَرُوهَا فَأَصْبَحُوا نَادِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فَأَخَذَهُمُ الْعَذَابُ ۗ إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "كَذَّبَتْ قَوْمُ لُوطٍ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "إِذْ قَالَ لَهُمْ أَخُوهُمْ لُوطٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "أَتَأْتُونَ الذُّكْرَانَ مِنَ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُمْ مِنْ أَزْوَاجِكُمْ ۚ بَلْ أَنْتُمْ قَوْمٌ عَادُونَ"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قَالُوا لَئِنْ لَمْ تَنْتَهِ يَا لُوطُ لَتَكُونَنَّ مِنَ الْمُخْرَجِينَ"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قَالَ إِنِّي لِعَمَلِكُمْ مِنَ الْقَالِينَ"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رَبِّ نَجِّنِي وَأَهْلِي مِمَّا يَعْمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فَنَجَّيْنَاهُ وَأَهْلَهُ أَجْمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "إِلَّا عَجُوزًا فِي الْغَابِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثُمَّ دَمَّرْنَا الْآخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وَأَمْطَرْنَا عَلَيْهِمْ مَطَرًا ۖ فَسَاءَ مَطَرُ الْمُنْذَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "كَذَّبَ أَصْحَابُ الْأَيْكَةِ الْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "إِذْ قَالَ لَهُمْ شُعَيْبٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "۞ أَوْفُوا الْكَيْلَ وَلَا تَكُونُوا مِنَ الْمُخْسِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُوا بِالْقِسْطَاسِ الْمُسْتَقِيمِ"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبْخَسُوا النَّاسَ أَشْيَاءَهُمْ وَلَا تَعْثَوْا فِي الْأَرْضِ مُفْسِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاتَّقُوا الَّذِي خَلَقَكُمْ وَالْجِبِلَّةَ الْأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوا إِنَّمَا أَنْتَ مِنَ الْمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا أَنْتَ إِلَّا بَشَرٌ مِثْلُنَا وَإِنْ نَظُنُّكَ لَمِنَ الْكَاذِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَأَسْقِطْ عَلَيْنَا كِسَفًا مِنَ السَّمَاءِ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّي أَعْلَمُ بِمَا تَعْمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ الظُّلَّةِ ۚ إِنَّهُ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَإِنَّهُ لَتَنْزِيلُ رَبِّ الْعَالَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ الرُّوحُ الْأَمِينُ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ الْمُنْذِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيٍّ مُبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَإِنَّهُ لَفِي زُبُرِ الْأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "أَوَلَمْ يَكُنْ لَهُمْ آيَةً أَنْ يَعْلَمَهُ عُلَمَاءُ بَنِي إِسْرَائِيلَ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوْ نَزَّلْنَاهُ عَلَىٰ بَعْضِ الْأَعْجَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَأَهُ عَلَيْهِمْ مَا كَانُوا بِهِ مُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذَٰلِكَ سَلَكْنَاهُ فِي قُلُوبِ الْمُجْرِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤْمِنُونَ بِهِ حَتَّىٰ يَرَوُا الْعَذَابَ الْأَلِيمَ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَأْتِيَهُمْ بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُولُوا هَلْ نَحْنُ مُنْظَرُونَ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "أَفَرَأَيْتَ إِنْ مَتَّعْنَاهُمْ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَاءَهُمْ مَا كَانُوا يُوعَدُونَ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَا أَغْنَىٰ عَنْهُمْ مَا كَانُوا يُمَتَّعُونَ"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَا أَهْلَكْنَا مِنْ قَرْيَةٍ إِلَّا لَهَا مُنْذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكْرَىٰ وَمَا كُنَّا ظَالِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وَمَا تَنَزَّلَتْ بِهِ الشَّيَاطِينُ"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنْبَغِي لَهُمْ وَمَا يَسْتَطِيعُونَ"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "إِنَّهُمْ عَنِ السَّمْعِ لَمَعْزُولُونَ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدْعُ مَعَ اللَّهِ إِلَٰهًا آخَرَ فَتَكُونَ مِنَ الْمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَأَنْذِرْ عَشِيرَتَكَ الْأَقْرَبِينَ"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الْمُؤْمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّي بَرِيءٌ مِمَّا تَعْمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وَتَوَكَّلْ عَلَى الْعَزِيزِ الرَّحِيمِ"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "الَّذِي يَرَاكَ حِينَ تَقُومُ"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِي السَّاجِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "إِنَّهُ هُوَ السَّمِيعُ الْعَلِيمُ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَنْ تَنَزَّلُ الشَّيَاطِينُ"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُلْقُونَ السَّمْعَ وَأَكْثَرُهُمْ كَاذِبُونَ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَالشُّعَرَاءُ يَتَّبِعُهُمُ الْغَاوُونَ"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "أَلَمْ تَرَ أَنَّهُمْ فِي كُلِّ وَادٍ يَهِيمُونَ"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَذَكَرُوا اللَّهَ كَثِيرًا وَانْتَصَرُوا مِنْ بَعْدِ مَا ظُلِمُوا ۗ وَسَيَعْلَمُ الَّذِينَ ظَلَمُوا أَيَّ مُنْقَلَبٍ يَنْقَلِبُونَ"
        }
    ]
}