{"pages":[{"chapter":112,"verse":1,"text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ هُوَ اللَّهُ أَحَدٌ"},{"chapter":112,"verse":2,"text":"اللَّهُ الصَّمَدُ"},{"chapter":112,"verse":3,"text":"لَمْ يَلِدْ وَلَمْ يُولَدْ"},{"chapter":112,"verse":4,"text":"وَلَمْ يَكُنْ لَهُ كُفُوًا أَحَدٌ"},{"chapter":113,"verse":1,"text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ"},{"chapter":113,"verse":2,"text":"مِنْ شَرِّ مَا خَلَقَ"},{"chapter":113,"verse":3,"text":"وَمِنْ شَرِّ غَاسِقٍ إِذَا وَقَبَ"},{"chapter":113,"verse":4,"text":"وَمِنْ شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ"},{"chapter":113,"verse":5,"text":"وَمِنْ شَرِّ حَاسِدٍ إِذَا حَسَدَ"},{"chapter":114,"verse":1,"text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ النَّاسِ"},{"chapter":114,"verse":2,"text":"مَلِكِ النَّاسِ"},{"chapter":114,"verse":3,"text":"إِلَٰهِ النَّاسِ"},{"chapter":114,"verse":4,"text":"مِنْ شَرِّ الْوَسْوَاسِ الْخَنَّاسِ"},{"chapter":114,"verse":5,"text":"الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ"},{"chapter":114,"verse":6,"text":"مِنَ الْجِنَّةِ وَالنَّاسِ"}]}