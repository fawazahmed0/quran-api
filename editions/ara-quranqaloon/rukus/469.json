{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "إِذَا وَقَعَتِ اِ۬لْوَاقِعَةُ"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "لَيْسَ لِوَقْعَتِهَا كَٰذِبَةٌ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "خَافِضَة ࣱ رَّافِعَةٌ"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "إِذَا رُجَّتِ اِ۬لْأَرْضُ رَجّا ࣰ‏"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "وَبُسَّتِ اِ۬لْجِبَالُ بَسّا ࣰ‏"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "فَكَانَتْ هَبَآء ࣰ مُّنۢبَثّا ࣰ‏"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "وَكُنتُمْ أَزْوَٰجا ࣰ ثَلَٰثَة ࣰ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "فَأَصْحَٰبُ اُ۬لْمَيْمَنَةِ مَا أَصْحَٰبُ اُ۬لْمَيْمَنَةِ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "وَأَصْحَٰبُ اُ۬لْمَشْـَٔمَةِ مَا أَصْحَٰبُ اُ۬لْمَشْـَٔمَةِ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "وَالسَّٰبِقُونَ اَ۬لسَّٰبِقُونَ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "أُوْلَٰٓئِكَ اَ۬لْمُقَرَّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "فِے جَنَّٰتِ اِ۬لنَّعِيمِ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ثُلَّة ࣱ مِّنَ اَ۬لْأَوَّلِينَ"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "وَقَلِيل ࣱ مِّنَ اَ۬لْأٓخِرِينَ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "عَلَيٰ سُرُر ࣲ مَّوْضُونَة ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "مُّتَّكِـِٕينَ عَلَيْهَا مُتَقَٰبِلِينَ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "يَطُوفُ عَلَيْهِمْ وِلْدَٰن ࣱ مُّخَلَّدُونَ"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "بِأَكْوَاب ࣲ وَأَبَارِيقَ وَكَأْس ࣲ مِّن مَّعِين ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزَفُونَ"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "وَفَٰكِهَة ࣲ مِّمَّا يَتَخَيَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "وَلَحْمِ طَيْر ࣲ مِّمَّا يَشْتَهُونَ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "وَحُورٌ عِين ࣱ‏"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "كَأَمْثَٰلِ اِ۬للُّؤْلُوِٕ اِ۬لْمَكْنُونِ"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "جَزَآءَۢ بِمَا كَانُواْ يَعْمَلُونَ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "لَا يَسْمَعُونَ فِيهَا لَغْوا ࣰ وَلَا تَأْثِيماً ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "إِلَّا قِيلا ࣰ سَلَٰما ࣰ سَلَٰما ࣰ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "وَأَصْحَٰبُ اُ۬لْيَمِينِ مَا أَصْحَٰبُ اُ۬لْيَمِينِ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "فِے سِدْر ࣲ مَّخْضُود ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "وَطَلْح ࣲ مَّنضُود ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "وَظِلّ ࣲ مَّمْدُود ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "وَمَآء ࣲ مَّسْكُوب ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "وَفَٰكِهَة ࣲ كَثِيرَة ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "لَّا مَقْطُوعَة ࣲ وَلَا مَمْنُوعَة ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "وَفُرُش ࣲ مَّرْفُوعَةٍ ۖ‏"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "إِنَّا أَنشَأْنَٰهُنَّ إِنشَآء ࣰ‏"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "فَجَعَلْنَٰهُنَّ أَبْكَاراً"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "عُرُباً أَتْرَابا ࣰ‏"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "لِّأَصْحَٰبِ اِ۬لْيَمِينِ ۖ‏"
        }
    ]
}