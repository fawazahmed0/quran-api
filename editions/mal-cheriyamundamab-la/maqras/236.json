{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "akasam pilarumpeal"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "at atinre raksitavin kilpetukayum ceyyumpeal-at (annane kilpetan) katappettirikkunnutanum"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "bhumi nittappetumpeal"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "atilullat at (purattekk‌) itukayum, at kaliyayittirukayum ceyyumpeal"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "atinre raksitavin at kilpetukayum ceyyumpeal- at (annane kilpetan) katappettirikkunnu tanum"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "he, manusya, ni ninre raksitavinkalekk katutta ad'dhvanam natatti cellunnavanum annane avanumayi kantumuttunnavanumakunnu"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "ennal (paraleakatt‌) etearuvann tanre rekha valatukaiyyil nalkappettuvea"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "avan laghuvaya vicaranaykk (matram) vidheyanakunnatan‌"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "avan avanre svantakkarute atuttekk santustanayikkeant tiriccupeakukayum ceyyum"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "ennal etearuvan tanre rekha avanre mutukinre pinnilute keatukkappettuvea"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "avan nasame enn nilavilikkukayum"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "alikkattunna narakagniyil katann eriyukayum ceyyum"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "tirccayayum avan avanre svantakkarkkitayil santeasatteate kaliyunnavanayirunnu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "tirccayayum avan dhariccu; avan matanni varunnate alla enn‌"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "ate, tirccayayum avanre raksitav avanepparri kantariyunnavanayirikkunnu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "ennal astamayaseabhayekkeant nan satyam ceytu parayunnu"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "ratriyum atu onnicc cerkkunnavayum keantum"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "candran purnnata prapikkumpeal atine keantum"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "tirccayayum ninnal ghattanghattamayi kayarikkeantirikkunnatan‌"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "ennal avarkkentuparri? avar visvasikkunnilla"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "avarkk khur'an otikeatukkappettal avar sujud ceyyunnumilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "pakse avisvasikal nisedhiccu tallukayan‌"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "avar manas'sukalil suksicc vekkunnatinepparri allahu nallavannam ariyunnavanakunnu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "akayal (nabiye,) ni avarkk vedanayeriya oru siksayepparri santeasavartta ariyikkuka"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "visvasikkukayum salkarm'mannal pravarttikkukayum ceytavarkkealike. avarkk murinnu peakatta pratiphalamunt‌"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "naksatramandalannalulla akasam tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vagdanam ceyyappetta a divasam tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "saksiyum saksyam vahikkappetunna karyavum tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "a kitanninre alkkar nasiccu peakatte"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "atayat viraku niracca tiyute alkkar"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avar atinkal irikkunnavarayirunna sandarbham"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "satyavisvasikalekkeant tannal ceyyunnatin avar drk‌saksikalayirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "pratapasaliyum stutyarhanumaya allahuvil avar visvasikkunnu ennat matramayirunnu avarute (satyavisvasikalute) mel avar (marddakar) cumattiya kurram"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "akasannaluteyum bhumiyuteyum mel adhipatyam ullavanumaya (allahuvil). allahu ellakaryattinum saksiyakunnu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "satyavisvasikaleyum satyavisvasinikaleyum marddikkukayum, pinnit pascattapikkatirikkukayum ceytavararea avarkku narakasiksayunt‌. tircca. avarkk cuttukarikkunna siksayunt‌"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "visvasikkukayum salkarm'mannal pravarttikkukayum ceytavararea avarkk talbhagattukuti aruvikal olukunna svargatteappukalunt‌; tircca. atatre valiya vijayam"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "tirccayayum ninre raksitavinre pituttam kathinamayat tanneyakunnu"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "tirccayayum avan tanneyan adyamayi untakkunnatum avartticc untakkunnatum"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avan ere pearukkunnavanum ere snehamullavanum"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "sinhasanattinre utamayum, mahatvamullavanum"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "tan uddesikkunnatentea at tikaccum pravarttikamakkunnavanuman‌"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "a sain'yannalute varttamanam ninakk vannukittiyirikkunnea"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "athava phir'aunreyum thamudinreyum (varttamanam)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "alla, satyanisedhikal nisedhiccu tallunnatilakunnu erpettittullat‌"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allahu avarute pinvasattukuti (avare) valayam ceytukeantirikkunnavanakunnu"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "alla, at mahatvameriya oru khur'anakunnu"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "sanraksitamaya oru phalakattilan atullat‌"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasam tanneyana, ratriyil varunnatu tanneyana satyam"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ratriyil varunnat ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tulacc kayarunna naksatramatre at‌"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "tanre karyattil oru melneattakkaranuntayikkeantallate yatearalumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "ennal manusyan cinticcu neakkatte tan entil ninnan srstikkappettirikkunnat enn‌"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "tericcu vilunna oru dravakattil ninnatre avan srstikkappettirikkunnat‌"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "mutukellinum, variyellukalkkumitayil ninn at purattu varunnu"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avane (manusyane) tiriccukeantu varan tirccayayum avan (allahu) kalivullavanakunnu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasyannal pariseadhikkappetunna divasam"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "appeal avan (manusyan‌) yatearu saktiyea, sahayiyea untayirikkukayilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "avartticc malapeyyikkunna akasattekkeantum"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sasyalatadikal mulappikkunna bhumiyekkeantum satyam"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "tirccayayum itu nirnayakamaya oru vakkakunnu"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "itu tamasayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "tirccayayum avar (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "nanum (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "akayal (nabiye,) ni satyanisedhikalkk kalatamasam nalkuka. alpasamayattekk avarkk tamasam nalkiyekkuka"
        }
    ]
}