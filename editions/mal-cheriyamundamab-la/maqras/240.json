{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "ennal avan ariyunnille? khab‌rukalilullat ilakkimaricc puratt keant varappetukayum"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "hrdayannalilullat velikk keantu varappetukayum ceytal"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "tirccayayum avarute raksitav anne divasam avare parri suksmamayi ariyunnavan tanneyakunnu"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhayankaramaya a sambhavam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "bhayankaramaya sambhavam ennal entakunnu"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "bhayankaramaya sambhavamennal entanenn ninakkariyumea"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "manusyanmar cinniccitariya parrayeppeale akunna divasam"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "parvvatannal katanna attin reamam pealeyum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appeal etearalute tulasukal ghanam tunniyea"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avan santrptamaya jivitattilayirikkum"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "ennal etearalute tulasukal tukkam kurannatayea"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avanre sanketam haviyah ayirikkum"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "haviyah ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "cuteriya narakagniyatre at‌"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "parasparam perumanatikkuka enna karyam ninnale asrad'dhayilakkiyirikkunnu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "ninnal savakutirannal sandarsikkunnat varekkum"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "nis'sansayam, ninnal valiye arinn keallum"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "pinneyum nis'sansayam ninnal valiye arinn keallum"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "nis'sansayam, ninnal drdhamaya ariv ariyumayirunnenkil"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "jvalikkunna narakatte ninnal kanuka tanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pinne tirccayayum ninnal atine drdhamayum kannal kanuka tanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "pinnit a divasattil sukhanubhavannale parri tirccayayum ninnal ceadyam ceyyappetukatanne ceyyum"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "kalam tanneyan satyam"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "tirccayayum manusyan nastattil tanneyakunnu"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "visvasikkukayum salkarm'mannal pravarttikkukayum, satyam kaikkeallan an'yean'yam upadesikkukayum ksama kaikkeallan an'yean'yam upadesikkukayum ceytavarealike"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "kuttuvakk parayunnavanum avahelikkunnavanumaya etearalkkum nasam"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "atayat dhanam sekharikkukayum at ennineakkikkeantirikkukayum ceyyunnavan‌"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "avanre dhanam avan sasvata jivitam nalkiyirikkunnu enn avan vicarikkunnu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "nis'sansayam, avan hutvamayil eriyappetuka tanne ceyyum"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutvama ennal entanenn ninakkariyamea"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "at allahuvinre jvalippikkappetta agniyakunnu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "hrdayannalilekk kattippatarunnataya"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "tirccayayum at avarute mel ataccumutappetunnatayirikkum"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "nittiyuntakkappetta stambhannalilayikkeant‌"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "anakkarekkeant ninre raksitav pravartticcat ennane enn ni kantille"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avarute tantram avan pilavilakkiyille"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "kuttankuttamayikkeantulla paksikale avarute nerkk avan ayakkukayum ceytu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "cuttupaluppicca kalimankallukalkeant avare eriyunnataya"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "annane avan avare tinneatukkappetta vaikkeal turumpupealeyakki"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "khurais geatratte kuttiyinakkiyatinal"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "saityakalatteyum usnakalatteyum yatrayumayi avare kuttiyinakkiyatinal"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "i bhavanattinre raksitavine avar aradhiccukeallatte"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "atayat avarkk visappinn aharam nalkukayum, bhayattin pakaram samadhanam nalkukayum ceytavane"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "matatte vyajamakkunnavan arenn ni kantuvea"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "anathakkuttiye tallikkalayunnavanatre at‌"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "pavappettavanre bhaksanattinre karyattil preatsahanam natattatirikkukayum ceyyunnavan"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "ennal namaskarakkarkkakunnu nasam"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "tannalute namaskarattepparri srad'dhayillattavaraya"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "janannale kanikkan venti pravarttikkunnavaraya"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "pareapakara vastukkal mutakkunnavarumaya"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "tirccayayum ninakk nam dharalam nettam nalkiyirikkunnu"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "akayal ni ninre raksitavin venti namaskarikkukayum baliyarppikkukayum ceyyuka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "tirccayayum ninneat vidvesam vecc pularttunnavan tanneyakunnu valarravan (bhaviyillattavan)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "(nabiye,) parayuka: avisvasikale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ninnal aradhiccuvarunnatine nan aradhikkunnilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nan aradhiccuvarunnatine ninnalum aradhikkunnavaralla"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ninnal aradhiccuvannatine nan aradhikkan peakunnavanumalla"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nan aradhiccu varunnatine ninnalum aradhikkan peakunnavaralla"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "ninnalkk ninnalute matam. enikk enre matavum"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "allahuvinre sahayavum vijayavum vannukittiyal"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "janannal allahuvinre matattil kuttankuttamayi pravesikkunnat ni kanukayum ceytal"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ninre raksitavine stutikkunnateateappam ni avane prakirttikkukayum, ni avaneat papameacanam tetukayum ceyyuka. tirccayayum avan pascattapam svikarikkunnavanakunnu"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "abulahabinre irukaikalum nasiccirikkunnu. avan nasamatayukayum ceytirikkunnu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avanre dhanamea avan sampadiccuveccatea avanu upakarappettilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "tijvalakalulla narakagniyil avan pravesikkunnatan‌"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "virakucumattukariyaya avanre bharyayum"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avalute kaluttil intappananarukeantulla oru kayaruntayirikkum"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "(nabiye,) parayuka: karyam allahu ekanan ennatakunnu"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allahu evarkkum asrayamayittullavanakunnu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avan (arkkum) janmam nalkiyittilla. (aruteyum santatiyayi) janiccittumilla"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "avann tulyanayi arum illatanum"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "parayuka: pulariyute raksitavineat nan saranam tetunnu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avan srsticcuttullavayute ketutiyil ninn‌"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "irulatayumpealulla ratriyute ketutiyilninnum"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "kettukalil utunna strikalute ketutiyilninnum"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "asuyalu asuyappetumpeal avanre ketutiyilninnum"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "parayuka: manusyarute raksitavineat nan saranam tetunnu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manusyarute rajavineat‌"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manusyarute daivatteat‌"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "durbeadhanam natatti pinmarikkalayunnavarekkeantulla ketutiyil ninn‌"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "manusyarute hrdayannalil durbeadhanam natattunnavar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "manusyarilum jinnukalilum pettavar"
        }
    ]
}