{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "a sambhavam sambhavicc kalinnal"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "atinre sambhavyataye nisedhikkunna arum untayirikkukayilla"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(a sambhavam, cilare) talttunnatum (cilare) uyarttunnatumayirikkum"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "bhumi kitukita virappikkappetukayum"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "parvvatannal iticc peatiyakkappetukayum"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "annane at paripparakkunna dhuliyayittirukayum"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "ninnal munn tarakkarayittirukayum ceyyunna sandarbhamatre at‌"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "appeal oru vibhagam valatupaksakkar. entan i valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "marrearu vibhagam itatupaksakkar. entan i itatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(satyavisvasattilum salpravrttikalilum) munneriyavar (paraleakattum) munneakkakkar tanne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "avarakunnu samipyam nalkappettavar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "sukhanubhutikalute svargatteappukalil"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "purvvikanmaril ninn oru vibhagavum"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "pilkkalakkaril ninn kuraccu perumatre ivar"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "svarnanulukeant metannuntakkappetta kattilukalil ayirikkum. avar"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avayil avar parasparam abhimukhamayi cariyirikkunnavarayirikkum"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "nityajivitam nalkappetta balanmar avarute itayil curri natakkum"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "keappakalum kujakalum sud'dhamaya uravu jalam niracca panapatravum keant‌"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "atu (kutikkuka) mulam avarkk talavedanayuntavukayea, lahari badhikkukayea illa"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "avar istappettu terannetukkunna tarattil petta palavargannalum"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "avar keatikkunna tarattil petta paksimansavum keant (avar curri natakkum)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "visalamaya nayanannalulla velutta tarunikalum. (avarkkunt‌)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(cippikalil) oliccu vekkappetta muttupealeyullavar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avar pravartticc keantirunnatinulla pratiphalamayikeantan (atellam nalkappetunnat‌)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "anavasyavakkea kurrappetuttalea avar avite vecc kelkkukayilla"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samadhanam! samadhanam! ennulla vakkallate"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "valatupaksakkar! entani valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mullilatta ilantamaram"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "atukkatukkayi kulakalulla vala"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "visalamaya tanal"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "sada olukkappettu keantirikkunna vellam"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "dharalam palavargannal"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nilaccu peavattatum tatas'sappettupeavattatumaya"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "uyarnnamettakal enni sukhanubhavannalilayirikkum avar"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "tirccayayum avare (svargastrikale) nam oru pratyeka prakrtiyeate srsticcuntakkiyirikkukayan‌"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "annane avare nam kan'yakamarakkiyirikkunnu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "snehavatikalum samaprayakkarum akkiyirikkunnu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "valatupaksakkarkk ventiyatre at‌"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "purvvikanmaril ninn oru vibhagavum"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "pingamikalil ninn oru vibhagavum ayirikkum avar"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "itatupaksakkar, entani itatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "tulaccu kayarunna usnakarr‌, cuttutilakkunna vellam"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "karimpukayute tanal"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "tanuppullatea, sukhadayakamea allatta (enni duritannalilayirikkum avar)"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "entukeantennal tirccayayum avar atinu mump sukhalealupanmarayirunnu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "avar gurutaramaya papattil sathiccunilkkunnavarumayirunnu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "avar iprakaram parayukayum ceytirunnu: nannal mariccitt mannum asthisakalannalumayikkalinnittanea nannal uyirttelunnelpikkappetan peakunnat‌"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "nannalute purvvikaraya pitakkalum (uyirttelunnelpikkappetumennea)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "ni parayuka: tirccayayum purvvikarum pilkkalakkarum ellam"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "oru niscita divasatte krtyamaya oru avadhikk orumiccukuttappetunnavar tanneyakunnu"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ennitt‌, he; satyanisedhikalaya durmargikale"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "tirccayayum ninnal oru vrksattil ninn atayat sakhkhumil ninn bhaksikkunnavarakunnu"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "annane atil ninn vayarukal nirakkunnavarum"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "atinre mite tilaccupeallunna vellattil ninn kutikkunnavarumakunnu"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "annane dahiccu valanna ottakam kutikkunnapeale kutikkunnavarakunnu"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "itayirikkum pratiphalattinre nalil avarkkulla salkkaram"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naman ninnale srsticcirikkunnat‌. ennirikke ninnalentan (enre sandesannale) satyamayi angikarikkattat‌"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "appeal ninnal sravikkunna suklattepparri ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ninnalanea at srsticcuntakkunnat‌. atalla, namanea srstikarttav‌"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "nam ninnalkkitayil maranam kanakkakkiyirikkunnu. nam orikkalum tealpikkappetunnavanalla"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "(ninnalkku) pakaram ninnale pealullavare keantuvarikayum. ninnalkk arivillatta vidhattil ninnale (vintum) srsticcuntakkukayum ceyyunna karyattil"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "adyatavana srstikkappettatinepparri tirccayayum ninnal manas'silakkiyittunt‌. ennittum ninnal entukeant aleaciccu neakkunnilla"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "ennal ninnal krsi ceyyunnatine parri ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ninnalanea at mulappiccu valarttunnat‌. atalla namanea, at mulappicc valarttunnavan"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "nam uddesiccirunnenkil at (vila) nam turumpakkittirkkumayirunnu. appeal ninnal atisayappettu parannukeanetayirikkumayirannu"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "tirccayayum nannal katabadhitar tanneyakunnu"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "alla, nannal (upajivana margam) tatayappettavarakunnu enn‌"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "ini, ninnal kutikkunna vellattepparri ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ninnalanea at meghattin ninn irakkiyat‌? atalla, namanea irakkiyavan"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "nam uddesiccirunnenkil at nam dus'svadulla uppuvellamakkumayirunnu. ennirikke ninnal nandikanikkattatentan‌"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "ninnal urasikattikkunnataya tiyine parri ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ninnalanea atinre maram srsticcuntakkiyat‌? atalla namanea srsticcuntakkiyavan"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "nam atine oru cintavisayamakkiyirikkunnu. daridraraya sancarikalkk oru jivitasekaryavum"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "akayal ninre mahanaya raksitavinre namatte ni prakirttikkuka"
        }
    ]
}