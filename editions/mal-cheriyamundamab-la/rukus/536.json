{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "ninakk ninre hrdayam nam visalatayullatakki tannille"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ninnil ninn ninre a bharam nam irakkivekkukayum ceytu"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ninre mutukine nericcu kalannataya (bharam)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ninakk ninre kirtti nam uyarttittarikayum ceytirikkunnu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "ennal tirccayayum nerukkattinre kute oru eluppamuntayirikkum"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "tirccayayum nerukkattinre kute oru eluppamuntayirikkum"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "akayal ninakk oliv kittiyal ni ad'dhvanikkuka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ninre raksitavilekk tanne ninre agraham samarppikkukayum ceyyuka"
        }
    ]
