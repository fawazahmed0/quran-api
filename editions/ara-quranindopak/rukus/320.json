{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "اِذۡ قَالَ لِاَبِيۡهِ وَقَوۡمِهٖ مَا تَعۡبُدُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُوۡا نَعۡبُدُ اَصۡنَامًا فَنَظَلُّ لَهَا عٰكِفِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلۡ يَسۡمَعُوۡنَكُمۡ اِذۡ تَدۡعُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "اَوۡ يَنۡفَعُوۡنَكُمۡ اَوۡ يَضُرُّوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُوۡا بَلۡ وَجَدۡنَا٘ اٰبَآءَنَا كَذٰلِكَ يَفۡعَلُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ اَفَرَءَيۡتُمۡ مَّا كُنۡتُمۡ تَعۡبُدُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "اَنۡتُمۡ وَاٰبَآؤُكُمُ الۡاَقۡدَمُوۡنَ ؗ‏"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَاِنَّهُمۡ عَدُوٌّ لِّيۡ٘ اِلَّا رَبَّ الۡعٰلَمِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "الَّذِيۡ خَلَقَنِيۡ فَهُوَ يَهۡدِيۡنِ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَالَّذِيۡ هُوَ يُطۡعِمُنِيۡ وَيَسۡقِيۡنِ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَاِذَا مَرِضۡتُ فَهُوَ يَشۡفِيۡنِ ࣕ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَالَّذِيۡ يُمِيۡتُنِيۡ ثُمَّ يُحۡيِيۡنِ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَالَّذِيۡ٘ اَطۡمَعُ اَنۡ يَّغۡفِرَ لِيۡ خَطِيۡٓـَٔتِيۡ يَوۡمَ الدِّيۡنِ ؕ‏"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِيۡ حُكۡمًا وَّاَلۡحِقۡنِيۡ بِالصّٰلِحِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاجۡعَلۡ لِّيۡ لِسَانَ صِدۡقٍ فِي الۡاٰخِرِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاجۡعَلۡنِيۡ مِنۡ وَّرَثَةِ جَنَّةِ النَّعِيۡمِ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاغۡفِرۡ لِاَبِيۡ٘ اِنَّهٗ كَانَ مِنَ الضَّآلِّيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخۡزِنِيۡ يَوۡمَ يُبۡعَثُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوۡمَ لَا يَنۡفَعُ مَالٌ وَّلَا بَنُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "اِلَّا مَنۡ اَتَي اللّٰهَ بِقَلۡبٍ سَلِيۡمٍ ؕ‏"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَاُزۡلِفَتِ الۡجَنَّةُ لِلۡمُتَّقِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ الۡجَحِيۡمُ لِلۡغٰوِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيۡلَ لَهُمۡ اَيۡنَمَا كُنۡتُمۡ تَعۡبُدُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِنۡ دُوۡنِ اللّٰهِ ؕ هَلۡ يَنۡصُرُوۡنَكُمۡ اَوۡ يَنۡتَصِرُوۡنَ ؕ‏"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبۡكِبُوۡا فِيۡهَا هُمۡ وَالۡغَاوٗنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُوۡدُ اِبۡلِيۡسَ اَجۡمَعُوۡنَ ؕ‏"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُوۡا وَهُمۡ فِيۡهَا يَخۡتَصِمُوۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاللّٰهِ اِنۡ كُنَّا لَفِيۡ ضَلٰلٍ مُّبِيۡنٍ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "اِذۡ نُسَوِّيۡكُمۡ بِرَبِّ الۡعٰلَمِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا٘ اَضَلَّنَا٘ اِلَّا الۡمُجۡرِمُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِنۡ شَافِعِيۡنَ ۙ‏"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيۡقٍ حَمِيۡمٍ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوۡ اَنَّ لَنَا كَرَّةً فَنَكُوۡنَ مِنَ الۡمُؤۡمِنِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةً ؕ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ࣖ‏"
        }
    ]
}