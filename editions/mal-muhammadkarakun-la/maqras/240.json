{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "avan ariyunnille? khabarukalilullava ilakkimarikkappetukayum"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "hrdayannalilullava velivakkappetukayum ceyyumpeal"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sansayamilla; annalil avarute nathan avarepparri suksmamayi ariyunnavanan"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhayankara sambhavam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "entana bhayankara sambhavam"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "a bhayankara sambhavam etenn ninakkentariyam"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ann manusyar cinniccitariya parrapealeyakum"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "parvatannal katanna kampili reamam pealeyum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appeal arute tulasinre tatt kanam tunnunnuvea"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avan santrptamaya jivitamunt"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "arute tulasin tatt kanam kurayunnuvea"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avanre sanketam haviya ayirikkum"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "haviya etenn ninakkentariyam"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "at keatum cutulla narakatti tanne"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "parasparam perumanatikkal ninnale asrad'dharakkiyirikkunnu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "ninnal savakkulikal sandarsikkum vare"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "sansayam venta; ninnalatariyukatanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "vintum sansayam venta; ninnalatariyukatanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "nis'sansayam! ninnal drdhamaya ariv netiyirunnenkil"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "narakatte ninnal neril kanukatanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pinne ninnalatine urappayum kannukeant kanukatanne ceyyum"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "pinnit ninnalanubhavicca sukhanugrahannalepparri ann ninnale ceadyam ceyyum"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "kalam saksi"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "tirccayayum manusyareakkeyum nastattilan"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "satyavisvasam svikariccavarum salkkarmannal pravartticcavarum; satyam svikarikkanum ksama palikkanum parasparamupadesiccavarumealike"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "kuttuvakk parayunnavaneakkeyum nasam! avahelikkunnavannum"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanea, dhanam orukkuttukayum at ennikkanakkakkukayum ceyyunnavanan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "dhanam tanne anasvaranakkiyatayi avan karutunnu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "sansayam venta; avan hutvamayil eriyappetuka tanne ceyyum"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutvama entenn ninakkariyamea"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "allahuvinre kattippatarum narakattiyanat"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "hrdayannalilekk kattippatarunnat"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "at avarute mel mutiyirikkum"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "nattinirttiya stambhannalil avar bandhitarayirikke"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "anakkare ninre nathan ceytatennaneyenn ni kantille"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avarute kutantram avan palakkiyille"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "avarute nere avan paksikkuttannale ayaccu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "cuttetutta kallukalkeant a paravakal avare erinnukeantirunnu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "annane allahu avare cavaccaracca kaccitturumpupealeyakki"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "khuraisikale inakkiyatinal"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "athava saityakalatteyum usnakalatteyum yatrayeatulla avarute inakkam"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "atinal i ka'aîbamandirattinre nathan avar valippetatte"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avarkk visappatakkan aharavum petikku pakaram nirbhayatvavum nalkiyavananavan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "matatte nisedhikkunnavane ni kantea"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "at anathaye attiyakarrunnavanan"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "agatiyute annam keatukkan prerippikkattavanum"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "atinal namaskarakkarkk nasam"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarea, tannalute namaskara karyattil asrad'dharan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avar alukale kanikkan venti ceyyunnavaran"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "nis'saramaya sahayam pealum mutakkunnavarum"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "ninakku nam dharalam nanma nalkiyirikkunnu"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "atinal ni ninre nathann namaskarikkuka. avann baliyarppikkuka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "niscayam ninneat satruta pularttunnavan tanneyan valarravan"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "parayuka: allayea satyanisedhikale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ninnal aradhikkunnavaye nan aradhikkunnilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nan aradhikkunnatine aradhikkunnavaralla ninnal"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ninnal aradhikkunnavaye aradhikkunnavanalla nan"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nan aradhikkunnatine aradhikkunnavaralla ninnalum"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "ninnalkk ninnalute matam. enikk enre matam"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "allahuvinre sahayavum vijayavum vannettiyal"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "janam kuttankuttamayi daivika matattil katannuvarunnat ni kanukayum ceytal"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ninre nathane ni stuticc valttuka. avaneat papameacanam tetuka. tirccayayum avan pascattapam svikarikkunnavanan"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "abulahabinre iru karannalum nasikkatte. avan nasiccirikkunnu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avanre svattea avan sampadiccatea avanneattum upakariccilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "alikkattunna narakattilavan cennettum"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "virak cumakkunna avanre bharyayum"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avalute kaluttil intappana narukeantulla kayarunt"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "parayuka, avanan allahu. avan ekanan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allahu areyum asrayikkattavanan. evaralum asrayikkappetunnavanum"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avan pitavea putranea alla"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "avanu tulyanayi arumilla"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "parayuka: prabhatattinre nathaneat nan saranam tetunnu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avan srsticcavayute dreahattilninn"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "irul mutumpealatte ravinre dreahattilninn"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "kettukalil utunnavarute dreahattilninn"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "asuyalu asuya kaniccalulla dreahattilninn"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "parayuka: nan saranam tetunnu, janannalute nathaneat"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "janannalute rajavineat"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "janannalute aradhyaneat"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "durbeadhanam natatti pinmarunna pisacinre dreahattilninn"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avan, manusya manas'sukalil durbeadhanam natattunnavanan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "manusyarilum jinnukalilum pettavanum"
        }
    ]
}