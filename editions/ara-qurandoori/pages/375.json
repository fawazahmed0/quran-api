{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاَتَّقُواْ اُ۬لَّذِي خَلَقَكُمۡ وَاَلۡجِبِلَّةَ اَ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ اَ۬لۡمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَآ أَنتَ إِلَّا بَشَر ࣱ مِّثۡلُنَا وَإِن نَّظُنُّكَ لَمِنَ اَ۬لۡكَٰذِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَأَسۡقِطۡ عَلَيۡنَا كِسۡف ࣰ ا مِّنَ اَ۬لسَّمَآ إِن كُنتَ مِنَ اَ۬لصَّٰدِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيَ أَعۡلَمُ بِمَا تَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوهُ فَأَخَذَهُمۡ عَذَابُ يَوۡمِ اِ۬لظُّلَّةِۚ إِنَّهُۥ كَانَ عَذَابَ يَوۡمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ اِ۬لرُّوحُ اُ۬لۡأَمِينُ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلَىٰ قَلۡبِكَ لِتَكُونَ مِنَ اَ۬لۡمُنذِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيّ ࣲ مُّبِين ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَإِنَّهُۥ لَفِي زُبُرِ اِ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "أَوَلَمۡ يَكُن لَّهُمۡ ءَايَةً أَن يَعۡلَمَهُۥ عُلَمَٰٓؤُاْ بَنِيٓ إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنَٰهُ عَلَىٰ بَعۡضِ اِ۬لۡأَعۡجَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَأَهُۥ عَلَيۡهِم مَّا كَانُواْ بِهِۦ مُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذَٰلِكَ سَلَكۡنَٰهُ فِي قُلُوبِ اِ۬لۡمُجۡرِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤۡمِنُونَ بِهِۦ حَتَّىٰ يَرَوُاْ اُ۬لۡعَذَابَ اَ۬لۡأَلِيمَ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَأۡتِيَهُم بَغۡتَة ࣰ وَهُمۡ لَا يَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُولُواْ هَلۡ نَحۡنُ مُنظَرُونَ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "أَفَبِعَذَابِنَا يَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "۞أَفَرَءَيۡتَ إِن مَّتَّعۡنَٰهُمۡ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُم مَّا كَانُواْ يُوعَدُونَ"
        }
    ]
