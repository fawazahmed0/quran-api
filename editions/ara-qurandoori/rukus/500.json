{
    "rukus": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "اِ۬لۡحَآقَّةُ"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "مَا اَ۬لۡحَآقَّةُ"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "وَمَآ أَدۡر۪ىٰكَ مَا اَ۬لۡحَآقَّةُ"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "كَذَّبَت ثَّمُودُ وَعَادُۢ بِالۡقَارِعَةِ"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِالطَّاغِيَةِ"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "وَأَمَّا عَاد ࣱ فَأُهۡلِكُواْ بِرِيح ࣲ صَرۡصَرٍ عَاتِيَة ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَال ࣲ وَثَمَٰنِيَةَ أَيَّامٍ حُسُوم ࣰ اۖ فَتَرَى اَ۬لۡقَوۡمَ فِيهَا صَرۡع ٜ يٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَة ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "فَهَل تَّر۪يٰ لَهُم مِّنۢ بَاقِيَة ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قِبَلَهُۥ وَاَلۡمُؤۡتَفِكَٰتُ بِالۡخَاطِئَةِ"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَة ࣰ رَّابِيَةً"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "إِنَّا لَمَّا طَغَا اَ۬لۡمَآءُ حَمَلۡنَٰكُمۡ فِي اِ۬لۡجَارِيَةِ"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَة ࣰ وَتَعِيَهَآ أُذُن ࣱ وَٰعِيَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "۞فَإِذَا نُفِخَ فِي اِ۬لصُّورِ نَفۡخَة ࣱ وَٰحِدَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "وَحُمِلَتِ اِ۬لۡأَرۡضُ وَاَلۡجِبَالُ فَدُكَّتَا دَكَّة ࣰ وَٰحِدَة ࣰ‏"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "فَيَوۡمَئِذ ࣲ وَقَعَتِ اِ۬لۡوَاقِعَةُ"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "وَاَنشَقَّتِ اِ۬لسَّمَآءُ فَهۡيَ يَوۡمَئِذ ࣲ وَاهِيَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "وَاَلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذ ࣲ ثَمَٰنِيَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "يَوۡمَئِذ ࣲ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ اُ۪قۡرَءُواْ كِتَٰبِيَهۡ"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "فَهۡوَ فِي عِيشَة ࣲ رَّاضِيَة ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "فِي جَنَّةٍ عَالِيَة ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "قُطُوفُهَا دَانِيَة ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "كُلُواْ وَاَشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي اِ۬لۡأَيَّامِ اِ۬لۡخَالِيَةِ"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "يَٰلَيۡتَهَا كَانَتِ اِ۬لۡقَاضِيَةَ"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَه"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "هَّلَكَ عَنِّي سُلۡطَٰنِيَهۡ"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "خُذُوهُ فَغُلُّوهُ"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "ثُمَّ اَ۬لۡجَحِيمَ صَلُّوهُ"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "ثُمَّ فِي سِلۡسِلَة ࣲ ذَرۡعُهَا سَبۡعُونَ ذِرَاع ࣰ ا فَاَسۡلُكُوهُ"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِاللَّهِ اِ۬لۡعَظِيمِ"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ اِ۬لۡمِسۡكِينِ"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "فَلَيۡسَ لَهُ اُ۬لۡيَوۡمَ هَٰهُنَا حَمِيم ࣱ‏"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِين ࣲ‏"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "لَّا يَأۡكُلُهُۥٓ إِلَّا اَ۬لۡخَٰطِـُٔونَ"
        }
    ]
}