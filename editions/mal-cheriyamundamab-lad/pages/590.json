{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "nakṣatramaṇḍalaṅṅaḷuḷḷa ākāśaṁ tanneyāṇa satyaṁ"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vāgdānaṁ ceyyappeṭṭa ā divasaṁ tanneyāṇa satyaṁ"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "sākṣiyuṁ sākṣyaṁ vahikkappeṭunna kāryavuṁ tanneyāṇa satyaṁ"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "ā kiṭaṅṅinṟe āḷkkār naśiccu pēākaṭṭe"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "atāyat viṟaku niṟacca tīyuṭe āḷkkār"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avar atiṅkal irikkunnavarāyirunna sandarbhaṁ"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "satyaviśvāsikaḷekkeāṇṭ taṅṅaḷ ceyyunnatin avar dr̥k‌sākṣikaḷāyirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "pratāpaśāliyuṁ stutyarhanumāya allāhuvil avar viśvasikkunnu ennat mātramāyirunnu avaruṭe (satyaviśvāsikaḷuṭe) mēl avar (marddakar) cumattiya kuṟṟaṁ"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "ākāśaṅṅaḷuṭeyuṁ bhūmiyuṭeyuṁ mēl ādhipatyaṁ uḷḷavanumāya (allāhuvil). allāhu ellākāryattinuṁ sākṣiyākunnu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "satyaviśvāsikaḷeyuṁ satyaviśvāsinikaḷeyuṁ marddikkukayuṁ, pinnīṭ paścāttapikkātirikkukayuṁ ceytavarārēā avarkku narakaśikṣayuṇṭ‌. tīrcca. avarkk cuṭṭukarikkunna śikṣayuṇṭ‌"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "viśvasikkukayuṁ salkarm'maṅṅaḷ pravarttikkukayuṁ ceytavarārēā avarkk tāḻbhāgattukūṭi aruvikaḷ oḻukunna svargattēāppukaḷuṇṭ‌; tīrcca. atatre valiya vijayaṁ"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "tīrccayāyuṁ ninṟe rakṣitāvinṟe piṭuttaṁ kaṭhinamāyat tanneyākunnu"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "tīrccayāyuṁ avan tanneyāṇ ādyamāyi uṇṭākkunnatuṁ āvartticc uṇṭākkunnatuṁ"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avan ēṟe peāṟukkunnavanuṁ ēṟe snēhamuḷḷavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "sinhāsanattinṟe uṭamayuṁ, mahatvamuḷḷavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "tān uddēśikkunnatentēā at tikaccuṁ prāvarttikamākkunnavanumāṇ‌"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "ā sain'yaṅṅaḷuṭe varttamānaṁ ninakk vannukiṭṭiyirikkunnēā"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "athavā phir'aunṟeyuṁ thamūdinṟeyuṁ (varttamānaṁ)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "alla, satyaniṣēdhikaḷ niṣēdhiccu taḷḷunnatilākunnu ērpeṭṭiṭṭuḷḷat‌"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allāhu avaruṭe pinvaśattukūṭi (avare) valayaṁ ceytukeāṇṭirikkunnavanākunnu"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "alla, at mahatvamēṟiya oru khur'ānākunnu"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "sanrakṣitamāya oru phalakattilāṇ atuḷḷat‌"
        }
    ]
}