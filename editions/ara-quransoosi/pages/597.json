{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَاَلتِّينِ وَاَلزَّيۡتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا اَ۬لۡبَلَدِ اِ۬لۡأَمِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا اَ۬لۡإِنسَٰنَ فِي أَحۡسَنِ تَقۡوِيم ࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا اَ۬لَّذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُون ࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِالدِّينِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيۡسَ اَ۬للَّهُ بِأَحۡكَمِ اِ۬لۡحَٰكِمِينَ"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "اِ۪قۡرَأۡ بِاسۡمِ رَبِّكَ اَ۬لَّذِي خَلَقَ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ اَ۬لۡإِنسَٰنَ مِنۡ عَلَقٍ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اِ۪قۡرَأۡ وَرَبُّكَ اَ۬لۡأَكۡرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "اُ۬لَّذِي عَلَّم بِالۡقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ اَ۬لۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّا إِنَّ اَ۬لۡإِنسَٰنَ لَيَطۡغ ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "أَن رَّء۪اهُ اُ۪سۡتَغۡن ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "إِنَّ إِلَىٰ رَبِّكَ اَ۬لرُّجۡع ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "أَرَءَيۡتَ اَ۬لَّذِي يَنۡه ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبۡدًا إِذَا صَلّ ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَءَيۡتَ إِن كَانَ عَلَى اَ۬لۡهُد ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوۡ أَمَرَ بِالتَّقۡو ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلّ ٜ يٰ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "أَلَمۡ يَعۡلَم بِأَنَّ اَ۬للَّهَ يَر۪يٰ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِالنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَة ࣲ كَٰذِبَةٍ خَاطِئَة ࣲ‏"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلۡيَدۡعُ نَادِيَهُۥ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدۡعُ اُ۬لزَّبَانِيَةَ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّا لَا تُطِعۡهُ وَاَسۡجُدۡۤ وَاَقۡتَرِب۩"
        }
