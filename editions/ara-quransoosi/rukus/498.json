{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "نٓۚ وَاَلۡقَلَمِ وَمَا يَسۡطُرُونَ"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "مَا أَنتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُون ࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "وَإِنَّ لَكَ لَأَجۡرًا غَيۡرَ مَمۡنُون ࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيم ࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "فَسَتُبۡصِرُ وَيُبۡصِرُونَ"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "بِأَييِّكُمُ اُ۬لۡمَفۡتُونُ"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "إِنَّ رَبَّكَ هُوَ أَعۡلَم بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهۡوَ أَعۡلَم بِالۡمُهۡتَدِينَ"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "فَلَا تُطِعِ اِ۬لۡمُكَذِّبِينَ"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "وَدُّواْ لَوۡ تُدۡهِنُ فَيُدۡهِنُونَ"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "وَلَا تُطِعۡ كُلَّ حَلَّاف ࣲ مَّهِينٍ"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "هَمَّاز ࣲ مَّشَّآءِۢ بِنَمِيم ࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "مَّنَّاع ࣲ لِّلۡخَيۡرِ مُعۡتَدٍ أَثِيمٍ"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "عُتُلِّۢ بَعۡدَ ذَٰلِكَ زَنِيمٍ"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "أَن كَانَ ذَا مَال ࣲ وَبَنِينَ"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ اُ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "سَنَسِمُهُۥ عَلَى اَ۬لۡخُرۡطُومِ"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَا أَصۡحَٰبَ اَ۬لۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "وَلَا يَسۡتَثۡنُونَ"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "فَطَافَ عَلَيۡهَا طَآئِف ࣱ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "فَأَصۡبَحَتۡ كَاَلصَّرِيمِ"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "فَتَنَادَوۡاْ مُصۡبِحِينَ"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "أَنِ اِ۟غۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "فَاَنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "أَن لَّا يَدۡخُلَنَّهَا اَ۬لۡيَوۡمَ عَلَيۡكُم مِّسۡكِين ࣱ‏"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "وَغَدَوۡاْ عَلَىٰ حَرۡد ࣲ قَٰدِرِينَ"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "فَلَمَّا رَأَوۡهَا قَالُواْ إِنَّا لَضَآلُّونَ"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "قَالُواْ سُبۡحَٰنَ رَبِّنَا إِنَّا كُنَّا ظَٰلِمِينَ"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡض ࣲ يَتَلَٰوَمُونَ"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "قَالُواْ يَٰوَيۡلَنَا إِنَّا كُنَّا طَٰغِينَ"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "عَسَىٰ رَبُّنَا أَن يُبَدِّلَنَا خَيۡر ࣰ ا مِّنۡهَا إِنَّا إِلَىٰ رَبِّنَا رَٰغِبُونَ"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "كَذَٰلِكَ اَ۬لۡعَذَابُۖ وَلَعَذَابُ اُ۬لۡأٓخِرَةِ أَكۡبَرۚ لَّوۡ كَانُواْ يَعۡلَمُونَ"
        }
    ]
}