{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَاَلضُّح ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَاَلَّيۡلِ إِذَا سَج ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَل ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡأٓخِرَةُ خَيۡر ࣱ لَّكَ مِنَ اَ۬لۡأُول ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡض ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمۡ يَجِدۡكَ يَتِيم ࣰ ا فَـَٔاو ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلّ ࣰ ا فَهَد ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِل ࣰ ا فَأَغۡن ٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا اَ۬لۡيَتِيمَ فَلَا تَقۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا اَ۬لسَّآئِلَ فَلَا تَنۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
        }
    ]
