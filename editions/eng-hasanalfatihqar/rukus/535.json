{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "By the midmorning"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "and by the night when it covers"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "your Lord has not forsaken you (Prophet Muhammad), nor does He hate you"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "The Last shall be better for you than the First"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Your Lord will give you, and you will be satisfied"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Did He not find you an orphan and give you shelter"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Did He not find you a wanderer so He guided you"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Did He not find you poor and suffice you"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Do not oppress the orphan"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "nor drive away the one who asks"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "But tell of the favors of your Lord"
        }
    ]
}