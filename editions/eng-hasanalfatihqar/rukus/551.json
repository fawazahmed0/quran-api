    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say: 'O unbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor do you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Nor am I worshiping what you have worshipped"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "neither will you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "To you your religion, and to me my Religion"
        }
    ]
