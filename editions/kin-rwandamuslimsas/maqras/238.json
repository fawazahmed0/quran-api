{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ndahiye uyu mujyi (wa Maka)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Nawe (Ntumwa Muhamadi) utuye uyu mujyi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "N’umubyeyi (Adamu) n’abo yabyaye"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Mu by’ukuri twaremye umuntu (agomba kunyura) mu ngorane"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ese akeka ko nta n’umwe wamushobora"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Aravuga ati “Nakoresheje umutungo mwinshi.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ese akeka ko nta n’umwe umubona"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ese ntitwamuhaye amaso abiri"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ndetse n’ururimi n’iminwa ibiri"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Tukanamwereka inzira ebyiri (iy’icyiza n’ikibi)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ariko ntiyigeze ashaka kunyura mu nzira igoranye (iganisha ku byiza n’intsinzi)"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ese ni iki cyakumenyesha iyo nzira igoranye"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(Ni) ukubohora umucakara"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Cyangwa gutanga amafunguro ku munsi w’amapfa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "(Ayo mafunguro ukayaha) imfubyi mufitanye isano rya bugufi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Cyangwa umukene wambaye ubusa (ubayeho mu kaga)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Hanyuma akaba umwe mu bemeramana, bakanagirana inama zo kwihangana ndetse bakanagirana inama zo kugira impuhwe"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Abo ni abo mu kuboko kw’iburyo (mu Ijuru)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Naho abahakanye amagambo yacu, ni abo mu kuboko kw’ibumoso (mu muriro)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Bazaba bafungiranye mu muriro (badafite aho binjirira n’aho basohokera)"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ndahiye izuba n’urumuri rwaryo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "N’ukwezi igihe kurikurikiye"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "N’amanywa igihe agaragaje urumuri (rw’izuba)"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "N’ijoro igihe riritwikiriye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "N’ikirere ndetse n’Uwagihanze"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "N’isi n’Uwayirambuye"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Na roho n’uwayitunganyije"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Hanyuma akayigaragariza ikibi n’icyiza"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Mu by’ukuri uweza roho ye (akubaha Allah kandi agakora ibikorwa byiza) yaratsinze"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Kandi rwose uwatereranye roho ye ari mu gihombo"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Abantu ba Thamudu bahinyuye (Intumwa yabo) kubera kwigomeka"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Ubwo ubarusha ububi yahagurukanaga ibakwe (akajya kwica ingamiya y’imbyeyi)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Nuko Intumwa ya Allah (Swaleh) irababwira iti “Muramenye! Iyo ni ingamiya ya Allah (ntimuyigirire nabi), kandi ntimuyibuze gushoka!”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Ariko baramuhinyuye (ya ngamiya) barayica. Nuko Nyagasani wabo arabarimbura ku bw’icyaha cyabo, ndetse bose abarimbura mu buryo bureshya (baba abakuru n’abato, abakire n’abakene, ndetse n’abakomeye n’abaciriritse)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Kandi (Allah) ntatinya ingaruka zabyo"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ndahiye ijoro igihe riguye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "N’amanywa igihe atangaje"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "N’uwaremye ikigabo n’ikigore"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Mu by’ukuri ibikorwa byanyu biratandukanye"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Wa wundi utanga (amaturo) akanagandukira (Allah)"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Kandi akizera icyiza (Ijuru)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tuzamworohereza inzira y’ibyiza"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ariko uzaba umunyabugugu, akirata yumva ko yishoboye"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Akanahinyura icyiza"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tuzamworohereza inzira y’ibibi"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Kandi ubwo azaba arimbuka (ajyanywe mu muriro), umutungo we nta cyo uzamumarira"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mu by’ukuri ukuyobora ni ukwacu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Kandi mu by’ukuri ni natwe tugenga iby’imperuka n’ibibanza (iby’isi)"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Bityo, mbaburiye umuriro ugurumana"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Nta we uzawinjiramo utari inkozi y’ibibi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Wa wundi wahinyuye (ukuri) akanagutera umugongo"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ariko ukora ibikorwa byiza, azawurindwa"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Wa wundi utanga umutungo we kugira ngo yiyeze"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Kandi ntashishikazwe no kwiturwa n’uwo yagiriye neza"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ahubwo (akabikora agamije) gushaka kwishimirwa na Nyagasani we, Uwikirenga"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Kandi rwose azishima (ubwo azinjira mu Ijuru)"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ndahiye agasusuruko"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "N’ijoro igihe ryijimye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "(Yewe Muhamadi), Nyagasani wawe ntiyagutereranye kandi nta n’ubwo yakwanze"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Kandi rwose ubuzima bw’imperuka ni bwo bwiza kuri wowe kurusha ububanza (ubw’isi)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Mu by’ukuri Nyagasani wawe azaguha kandi uzishima"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ese ntiyagusanze uri imfubyi akaguha icumbi (abakwitaho)"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Akanagusanga udasobanukiwe (Qur’an n’amategeko yayo), nuko akakuyobora"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Akaba yaranagusanze uri umukene maze akagukungahaza (akaguha kunyurwa)"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Bityo, ntugahutaze imfubyi"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ntukanakankamire uje asaba"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Kandi ingabire za Nyagasani wawe (yaguhaye), ujye uzigaragaza (umushimira)"
        }
    ]
}