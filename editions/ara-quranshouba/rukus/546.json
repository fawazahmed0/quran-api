{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَيۡل ࣱ لِّكُلِّ هُمَزَة ࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ٱلَّذِي جَمَعَ مَال ࣰ ا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَآ أَدۡرٜىٰكَ مَا ٱلۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيۡهِم مُّوصَدَة ࣱ‏"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِي عُمُد ࣲ مُّمَدَّدَةِۭ"
        }
    ]
}