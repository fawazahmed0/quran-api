{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "وَٱلۡأٓخِرَةُ خَيۡر ࣱ وَأَبۡقَىٰٓ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وُجُوه ࣱ يَوۡمَئِذٍ خَٰشِعَةٌ"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عَامِلَة ࣱ نَّاصِبَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تُصۡلَىٰ نَارًا حَامِيَة ࣰ‏"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَة ࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيع ࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوع ࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وُجُوه ࣱ يَوۡمَئِذ ࣲ نَّاعِمَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لِّسَعۡيِهَا رَاضِيَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "فِي جَنَّةٍ عَالِيَة ࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَة ࣰ‏"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فِيهَا عَيۡن ࣱ جَارِيَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فِيهَا سُرُر ࣱ مَّرۡفُوعَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "وَأَكۡوَاب ࣱ مَّوۡضُوعَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "وَنَمَارِقُ مَصۡفُوفَة ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وَزَرَابِيُّ مَبۡثُوثَةٌ"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّر ࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَٱلۡفَجۡرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَيَالٍ عَشۡر ࣲ‏"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِي ذَٰلِكَ قَسَم ࣱ لِّذِي حِجۡرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡل ࣰ ا لَّمّ ࣰ ا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّ ࣰ ا جَمّ ࣰ ا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّ ࣰ ا دَكّ ࣰ ا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّ ࣰ ا صَفّ ࣰ ا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذ ࣲ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "فَيَوۡمَئِذ ࣲ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَد ࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَد ࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَة ࣰ مَّرۡضِيَّة ࣰ‏"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "فَٱدۡخُلِي فِي عِبَٰدِي"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "وَٱدۡخُلِي جَنَّتِي"
        }
    ]
}