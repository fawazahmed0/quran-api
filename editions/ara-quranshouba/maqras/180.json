{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "۞وَإِنَّ مِن شِيعَتِهِۦ لَإِبۡرَٰهِيمَ"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "إِذۡ جَآءَ رَبَّهُۥ بِقَلۡب ࣲ سَلِيمٍ"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "إِذۡ قَالَ لِأَبِيهِ وَقَوۡمِهِۦ مَاذَا تَعۡبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "أَئِفۡكًا ءَالِهَة ࣰ دُونَ ٱللَّهِ تُرِيدُونَ"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "فَمَا ظَنُّكُم بِرَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "فَنَظَرَ نَظۡرَة ࣰ فِي ٱلنُّجُومِ"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "فَقَالَ إِنِّي سَقِيم ࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "فَتَوَلَّوۡاْ عَنۡهُ مُدۡبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمۡ فَقَالَ أَلَا تَأۡكُلُونَ"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "مَا لَكُمۡ لَا تَنطِقُونَ"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "فَرَاغَ عَلَيۡهِمۡ ضَرۡبَۢا بِٱلۡيَمِينِ"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "فَأَقۡبَلُوٓاْ إِلَيۡهِ يَزِفُّونَ"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "قَالَ أَتَعۡبُدُونَ مَا تَنۡحِتُونَ"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "وَٱللَّهُ خَلَقَكُمۡ وَمَا تَعۡمَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "قَالُواْ ٱبۡنُواْ لَهُۥ بُنۡيَٰن ࣰ ا فَأَلۡقُوهُ فِي ٱلۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "فَأَرَادُواْ بِهِۦ كَيۡد ࣰ ا فَجَعَلۡنَٰهُمُ ٱلۡأَسۡفَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "وَقَالَ إِنِّي ذَاهِبٌ إِلَىٰ رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "رَبِّ هَبۡ لِي مِنَ ٱلصَّٰلِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "فَبَشَّرۡنَٰهُ بِغُلَٰمٍ حَلِيم ࣲ‏"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعۡيَ قَالَ يَٰبُنَيِّ إِنِّيٓ أَرَىٰ فِي ٱلۡمَنَامِ أَنِّيٓ أَذۡبَحُكَ فَٱنظُرۡ مَاذَا تَرَىٰۚ قَالَ يَٰٓأَبَتِ ٱفۡعَلۡ مَا تُؤۡمَرُۖ سَتَجِدُنِيٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "فَلَمَّآ أَسۡلَمَا وَتَلَّهُۥ لِلۡجَبِينِ"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "وَنَٰدَيۡنَٰهُ أَن يَٰٓإِبۡرَٰهِيمُ"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "قَدۡ صَدَّقۡتَ ٱلرُّءۡيَآۚ إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "إِنَّ هَٰذَا لَهُوَ ٱلۡبَلَٰٓؤُاْ ٱلۡمُبِينُ"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "وَفَدَيۡنَٰهُ بِذِبۡحٍ عَظِيم ࣲ‏"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "سَلَٰمٌ عَلَىٰٓ إِبۡرَٰهِيمَ"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "وَبَشَّرۡنَٰهُ بِإِسۡحَٰقَ نَبِيّ ࣰ ا مِّنَ ٱلصَّٰلِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "وَبَٰرَكۡنَا عَلَيۡهِ وَعَلَىٰٓ إِسۡحَٰقَۚ وَمِن ذُرِّيَّتِهِمَا مُحۡسِن ࣱ وَظَالِم ࣱ لِّنَفۡسِهِۦ مُبِين ࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّيۡنَٰهُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَرۡبِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرۡنَٰهُمۡ فَكَانُواْ هُمُ ٱلۡغَٰلِبِينَ"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَيۡنَٰهُمَا ٱلۡكِتَٰبَ ٱلۡمُسۡتَبِينَ"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَيۡنَٰهُمَا ٱلصِّرَٰطَ ٱلۡمُسۡتَقِيمَ"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكۡنَا عَلَيۡهِمَا فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَٰمٌ عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلۡيَاسَ لَمِنَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذۡ قَالَ لِقَوۡمِهِۦٓ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدۡعُونَ بَعۡل ࣰ ا وَتَذَرُونَ أَحۡسَنَ ٱلۡخَٰلِقِينَ"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ٱللَّهُ رَبُّكُمۡ وَرَبُّ ءَابَآئِكُمُ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِينَ"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي ٱلۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَٰمٌ عَلَىٰٓ إِلۡ يَاسِينَ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوط ࣰ ا لَّمِنَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذۡ نَجَّيۡنَٰهُ وَأَهۡلَهُۥٓ أَجۡمَعِينَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوز ࣰ ا فِي ٱلۡغَٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرۡنَا ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَيۡهِم مُّصۡبِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِٱلَّيۡلِۚ أَفَلَا تَعۡقِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "وَإِنَّ يُونُسَ لَمِنَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "إِذۡ أَبَقَ إِلَى ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "فَسَاهَمَ فَكَانَ مِنَ ٱلۡمُدۡحَضِينَ"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "فَٱلۡتَقَمَهُ ٱلۡحُوتُ وَهُوَ مُلِيم ࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "فَلَوۡلَآ أَنَّهُۥ كَانَ مِنَ ٱلۡمُسَبِّحِينَ"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "لَلَبِثَ فِي بَطۡنِهِۦٓ إِلَىٰ يَوۡمِ يُبۡعَثُونَ"
        }
    ]
}