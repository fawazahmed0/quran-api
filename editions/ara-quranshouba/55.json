{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "ٱلرَّحۡمَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ ٱلۡقُرۡءَانَ"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ ٱلۡبَيَانَ"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَان ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيهَا فَٰكِهَة ࣱ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰل ࣲ كَٱلۡفَخَّارِ"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِج ࣲ مِّن نَّار ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيۡنَهُمَا بَرۡزَخ ࣱ لَّا يَبۡغِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّولُؤُ وَٱلۡمَرۡجَانُ"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشِـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "كُلُّ مَنۡ عَلَيۡهَا فَان ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَٰلِ وَٱلۡإِكۡرَامِ"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "يَسۡـَٔلُهُۥ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ كُلَّ يَوۡمٍ هُوَ فِي شَأۡن ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "سَنَفۡرُغُ لَكُمۡ أَيُّهَ ٱلثَّقَلَانِ"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "يَٰمَعۡشَرَ ٱلۡجِنِّ وَٱلۡإِنسِ إِنِ ٱسۡتَطَعۡتُمۡ أَن تَنفُذُواْ مِنۡ أَقۡطَارِ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ فَٱنفُذُواْۚ لَا تَنفُذُونَ إِلَّا بِسُلۡطَٰن ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "يُرۡسَلُ عَلَيۡكُمَا شُوَاظ ࣱ مِّن نَّار ࣲ وَنُحَاس ࣱ فَلَا تَنتَصِرَانِ"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتۡ وَرۡدَة ࣰ كَٱلدِّهَانِ"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "فَيَوۡمَئِذ ࣲ لَّا يُسۡـَٔلُ عَن ذَنۢبِهِۦٓ إِنس ࣱ وَلَا جَآنّ ࣱ‏"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "يُعۡرَفُ ٱلۡمُجۡرِمُونَ بِسِيمَٰهُمۡ فَيُؤۡخَذُ بِٱلنَّوَٰصِي وَٱلۡأَقۡدَامِ"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "هَٰذِهِۦ جَهَنَّمُ ٱلَّتِي يُكَذِّبُ بِهَا ٱلۡمُجۡرِمُونَ"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "يَطُوفُونَ بَيۡنَهَا وَبَيۡنَ حَمِيمٍ ءَان ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ذَوَاتَآ أَفۡنَان ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "فِيهِمَا عَيۡنَانِ تَجۡرِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "فِيهِمَا مِن كُلِّ فَٰكِهَة ࣲ زَوۡجَانِ"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "مُتَّكِـِٔينَ عَلَىٰ فُرُشِۭ بَطَآئِنُهَا مِنۡ إِسۡتَبۡرَق ࣲ ۚ وَجَنَى ٱلۡجَنَّتَيۡنِ دَان ࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "فِيهِنَّ قَٰصِرَٰتُ ٱلطَّرۡفِ لَمۡ يَطۡمِثۡهُنَّ إِنس ࣱ قَبۡلَهُمۡ وَلَا جَآنّ ࣱ‏"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "كَأَنَّهُنَّ ٱلۡيَاقُوتُ وَٱلۡمَرۡجَانُ"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "هَلۡ جَزَآءُ ٱلۡإِحۡسَٰنِ إِلَّا ٱلۡإِحۡسَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "وَمِن دُونِهِمَا جَنَّتَانِ"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "مُدۡهَآمَّتَانِ"
