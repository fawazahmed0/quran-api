            "verse": 3,
            "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "بِأَيِّ ذَنۢب ࣲ قُتِلَتۡ"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "وَإِذَا ٱلۡجَحِيمُ سُعِرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "عَلِمَتۡ نَفۡس ࣱ مَّآ أَحۡضَرَتۡ"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "إِنَّهُۥ لَقَوۡلُ رَسُول ࣲ كَرِيم ࣲ‏"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِين ࣲ‏"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "مُّطَاع ࣲ ثَمَّ أَمِين ࣲ‏"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "وَمَا صَاحِبُكُم بِمَجۡنُون ࣲ‏"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "وَلَقَدۡ رٜءٜاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِين ࣲ‏"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰن ࣲ رَّجِيم ࣲ‏"
