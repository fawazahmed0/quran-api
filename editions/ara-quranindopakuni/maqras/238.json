{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لَا٘ اُقۡسِمُ بِهٰذَا الۡبَلَدِ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَاَنۡتَ حِلٌّ ۭ بِهٰذَا الۡبَلَدِ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدٍ وَّمَا وَلَدَ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا الۡاِنۡسَانَ فِيۡ كَبَدٍ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "اَيَحۡسَبُ اَنۡ لَّنۡ يَّقۡدِرَ عَلَيۡهِ اَحَدٌ ۘ ۝"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُوۡلُ اَهۡلَكۡتُ مَالًا لُّبَدًا ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "اَيَحۡسَبُ اَنۡ لَّمۡ يَرَهٗ٘ اَحَدٌ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "اَلَمۡ نَجۡعَلۡ لَّهٗ عَيۡنَيۡنِ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانًا وَّشَفَتَيۡنِ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيۡنٰهُ النَّجۡدَيۡنِ ۚ ۝"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا اقۡتَحَمَ الۡعَقَبَةَ ؗۖ ۝"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا الۡعَقَبَةُ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكُّ رَقَبَةٍ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "اَوۡ اِطۡعٰمٌ فِيۡ يَوۡمٍ ذِيۡ مَسۡغَبَةٍ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَّتِيۡمًا ذَا مَقۡرَبَةٍ ۙ ۝"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "اَوۡ مِسۡكِيۡنًا ذَا مَتۡرَبَةٍ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ الَّذِيۡنَ اٰمَنُوۡا وَتَوَاصَوۡا بِالصَّبۡرِ وَتَوَاصَوۡا بِالۡمَرۡحَمَةِ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "اُولٰٓئِكَ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَالَّذِيۡنَ كَفَرُوۡا بِاٰيٰتِنَا هُمۡ اَصۡحٰبُ الۡمَشۡـَٔمَةِ ؕ ۝"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيۡهِمۡ نَارٌ مُّؤۡصَدَةٌ ࣖ ۝"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَالشَّمۡسِ وَضُحٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَالۡقَمَرِ اِذَا تَلٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَالنَّهَارِ اِذَا جَلّٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَالسَّمَآءِ وَمَا بَنٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَالۡاَرۡضِ وَمَا طَحٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسٍ وَّمَا سَوّٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَاَلۡهَمَهَا فُجُوۡرَهَا وَتَقۡوٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ اَفۡلَحَ مَنۡ زَكّٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَنۡ دَسّٰىهَا ؕ ۝"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَتۡ ثَمُوۡدُ بِطَغۡوٰىهَا٘ ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "اِذِ انۡ ۭ بَعَثَ اَشۡقٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمۡ رَسُوۡلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقۡيٰهَا ؕ ۝"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوۡهُ فَعَقَرُوۡهَا ࣢ۙࣕ فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُمۡ بِذَنۡ ۭ بِهِمۡ فَسَوّٰىهَا ۙࣕ ۝"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا يَخَافُ عُقۡبٰهَا ࣖ ۝"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَالنَّهَارِ اِذَا تَجَلّٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ الذَّكَرَ وَالۡاُنۡثٰ٘ي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "اِنَّ سَعۡيَكُمۡ لَشَتّٰي ؕ ۝"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَاَمَّا مَنۡ اَعۡطٰي وَاتَّقٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِالۡحُسۡنٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهٗ لِلۡيُسۡرٰي ؕ ۝"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَاَمَّا مَنۡ ۭ بَخِلَ وَاسۡتَغۡنٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِالۡحُسۡنٰي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهٗ لِلۡعُسۡرٰي ؕ ۝"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغۡنِيۡ عَنۡهُ مَالُهٗ٘ اِذَا تَرَدّٰي ؕ ۝"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "اِنَّ عَلَيۡنَا لَلۡهُدٰي ؗۖ ۝"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَاِنَّ لَنَا لَلۡاٰخِرَةَ وَالۡاُوۡلٰي ۝"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَاَنۡذَرۡتُكُمۡ نَارًا تَلَظّٰي ۚ ۝"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصۡلٰىهَا٘ اِلَّا الۡاَشۡقَي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "الَّذِيۡ كَذَّبَ وَتَوَلّٰي ؕ ۝"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا الۡاَتۡقَي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "الَّذِيۡ يُؤۡتِيۡ مَالَهٗ يَتَزَكّٰي ۚ ۝"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِاَحَدٍ عِنۡدَهٗ مِنۡ نِّعۡمَةٍ تُجۡزٰ٘ي ۙ ۝"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "اِلَّا ابۡتِغَآءَ وَجۡهِ رَبِّهِ الۡاَعۡلٰي ۚ ۝"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوۡفَ يَرۡضٰي ࣖ ۝"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَالضُّحٰي ۙ ۝"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَالَّيۡلِ اِذَا سَجٰي ۙ ۝"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٰي ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡاٰخِرَةُ خَيۡرٌ لَّكَ مِنَ الۡاُوۡلٰي ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيۡكَ رَبُّكَ فَتَرۡضٰي ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "اَلَمۡ يَجِدۡكَ يَتِيۡمًا فَاٰوٰي ࣕ ۝"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلًّا فَهَدٰي ࣕ ۝"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلًا فَاَغۡنٰي ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَاَمَّا الۡيَتِيۡمَ فَلَا تَقۡهَرۡ ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَاَمَّا السَّآئِلَ فَلَا تَنۡهَرۡ ؕ ۝"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَاَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ ࣖ ۝"
        }
    ]
}