{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "اِذَا وَقَعَتِ الۡوَاقِعَةُ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ ۘ‏"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "خَافِضَةٌ رَّافِعَةٌ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "اِذَا رُجَّتِ الۡاَرۡضُ رَجًّا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "وَّبُسَّتِ الۡجِبَالُ بَسًّا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "فَكَانَتۡ هَبَآءً مُّنۡ ۭ بَثًّا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "وَّكُنۡتُمۡ اَزۡوَاجًا ثَلٰثَةً ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "فَاَصۡحٰبُ الۡمَيۡمَنَةِ ࣢ۙ مَا٘ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "وَاَصۡحٰبُ الۡمَشۡـَٔمَةِ ࣢ۙ مَا٘ اَصۡحٰبُ الۡمَشۡـَٔمَةِ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "وَالسّٰبِقُوۡنَ السّٰبِقُوۡنَ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "اُولٰٓئِكَ الۡمُقَرَّبُوۡنَ ۚ‏"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "فِيۡ جَنّٰتِ النَّعِيۡمِ"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "وَقَلِيۡلٌ مِّنَ الۡاٰخِرِيۡنَ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "عَلٰي سُرُرٍ مَّوۡضُوۡنَةٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "مُّتَّكِـِٕيۡنَ عَلَيۡهَا مُتَقٰبِلِيۡنَ"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "يَطُوۡفُ عَلَيۡهِمۡ وِلۡدَانٌ مُّخَلَّدُوۡنَ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "بِاَكۡوَابٍ وَّاَبَارِيۡقَ ࣢ۙ وَكَاۡسٍ مِّنۡ مَّعِيۡنٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "لَّا يُصَدَّعُوۡنَ عَنۡهَا وَلَا يُنۡزِفُوۡنَ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوۡنَ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "وَلَحۡمِ طَيۡرٍ مِّمَّا يَشۡتَهُوۡنَ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "وَحُوۡرٌ عِيۡنٌ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "كَاَمۡثَالِ اللُّؤۡلُؤِ الۡمَكۡنُوۡنِ ۚ‏"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "جَزَآءً ۭ بِمَا كَانُوۡا يَعۡمَلُوۡنَ"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَغۡوًا وَّلَا تَاۡثِيۡمًا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "اِلَّا قِيۡلًا سَلٰمًا سَلٰمًا"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "وَاَصۡحٰبُ الۡيَمِيۡنِ ࣢ۙ مَا٘ اَصۡحٰبُ الۡيَمِيۡنِ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "فِيۡ سِدۡرٍ مَّخۡضُوۡدٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "وَّطَلۡحٍ مَّنۡضُوۡدٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "وَّظِلٍّ مَّمۡدُوۡدٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "وَّمَآءٍ مَّسۡكُوۡبٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "وَّفَاكِهَةٍ كَثِيۡرَةٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "لَّا مَقۡطُوۡعَةٍ وَّلَا مَمۡنُوۡعَةٍ ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "وَّفُرُشٍ مَّرۡفُوۡعَةٍ ؕ‏"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "اِنَّا٘ اَنۡشَاۡنٰهُنَّ اِنۡشَآءً ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "فَجَعَلۡنٰهُنَّ اَبۡكَارًا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "عُرُبًا اَتۡرَابًا ۙ‏"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "لِّاَصۡحٰبِ الۡيَمِيۡنِ ࣖ ؕ‏"
        }
    ]
}