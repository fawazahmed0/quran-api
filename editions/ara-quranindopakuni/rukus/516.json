{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "وَالۡمُرۡسَلٰتِ عُرۡفًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فَالۡعٰصِفٰتِ عَصۡفًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "وَّالنّٰشِرٰتِ نَشۡرًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فَالۡفٰرِقٰتِ فَرۡقًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فَالۡمُلۡقِيٰتِ ذِكۡرًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عُذۡرًا اَوۡ نُذۡرًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "اِنَّمَا تُوۡعَدُوۡنَ لَوَاقِعٌ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فَاِذَا النُّجُوۡمُ طُمِسَتۡ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "وَاِذَا السَّمَآءُ فُرِجَتۡ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "وَاِذَا الۡجِبَالُ نُسِفَتۡ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "وَاِذَا الرُّسُلُ اُقِّتَتۡ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لِاَيِّ يَوۡمٍ اُجِّلَتۡ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "لِيَوۡمِ الۡفَصۡلِ ۚ‏"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وَمَا٘ اَدۡرٰىكَ مَا يَوۡمُ الۡفَصۡلِ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "اَلَمۡ نُهۡلِكِ الۡاَوَّلِيۡنَ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثُمَّ نُتۡبِعُهُمُ الۡاٰخِرِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "اَلَمۡ نَخۡلُقۡكُّمۡ مِّنۡ مَّآءٍ مَّهِيۡنٍ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فَجَعَلۡنٰهُ فِيۡ قَرَارٍ مَّكِيۡنٍ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "اِلٰي قَدَرٍ مَّعۡلُوۡمٍ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فَقَدَرۡنَا ࣗۖ فَنِعۡمَ الۡقٰدِرُوۡنَ"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ كِفَاتًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "اَحۡيَآءً وَّاَمۡوَاتًا ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَّجَعَلۡنَا فِيۡهَا رَوَاسِيَ شٰمِخٰتٍ وَّاَسۡقَيۡنٰكُمۡ مَّآءً فُرَاتًا ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "اِنۡطَلِقُوۡ٘ا اِلٰي مَا كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ ۚ‏"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "اِنۡطَلِقُوۡ٘ا اِلٰي ظِلٍّ ذِيۡ ثَلٰثِ شُعَبٍ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيۡلٍ وَّلَا يُغۡنِيۡ مِنَ اللَّهَبِ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "اِنَّهَا تَرۡمِيۡ بِشَرَرٍ كَالۡقَصۡرِ ۚ‏"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَاَنَّهٗ جِمٰلَتٌ صُفۡرٌ ؕ‏"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هٰذَا يَوۡمُ لَا يَنۡطِقُوۡنَ ۙ‏"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُوۡنَ"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هٰذَا يَوۡمُ الۡفَصۡلِ ۚ جَمَعۡنٰكُمۡ وَالۡاَوَّلِيۡنَ"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَاِنۡ كَانَ لَكُمۡ كَيۡدٌ فَكِيۡدُوۡنِ"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيۡلٌ يَّوۡمَئِذٍ لِّلۡمُكَذِّبِيۡنَ ࣖ‏"
        }
    ]
}