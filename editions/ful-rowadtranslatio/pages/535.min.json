{"pages":[{"chapter":56,"verse":17,"text":"Sukaaɓe duuminaaɓe hino wanngoo hakkunde maɓɓe"},{"chapter":56,"verse":18,"text":"wonndude e ñedduɗe, e cataloy, e koroy e [hebbiniraakoy] beere ilaynde"},{"chapter":56,"verse":19,"text":"ɓe muusetaake ko*e nder ton, ɓe siwritaa"},{"chapter":56,"verse":20,"text":"e muuyanteeɗi ɗi ɓe suɓotoo"},{"chapter":56,"verse":21,"text":"e teewu colli ngu ɓe tuuyatee"},{"chapter":56,"verse":22,"text":"e huurul'ayniiɓe -yaaja gite en"},{"chapter":56,"verse":23,"text":"nannduɓe e kaŋŋe lu'lu'u maraaɗe"},{"chapter":56,"verse":24,"text":"njoɓdi ko ɓe gollaynoo kon"},{"chapter":56,"verse":25,"text":"Ɓe nanataa nder ton haala meere, wanaa ko bakkondiŋta"},{"chapter":56,"verse":26,"text":"si wanaa wi'aneede : \"Salaaman! Salaaman!\" [Saɗi mon! saɗi mon]"},{"chapter":56,"verse":27,"text":"E wontirooɓe ñaamo ɓen, ko woni ɓen yimɓe ñaamo"},{"chapter":56,"verse":28,"text":"[Ɓe wonay] e leɗɗe jaaɓe newɗuɗe"},{"chapter":56,"verse":29,"text":"e banaanaje fawandirɗe"},{"chapter":56,"verse":30,"text":"e ɗowdi duumiindi"},{"chapter":56,"verse":31,"text":"e ndiyam juurayɗam"},{"chapter":56,"verse":32,"text":"e ɓiɗɓe leɗɗe ɗuuɗuɗe"},{"chapter":56,"verse":33,"text":"ɗe taƴondirtaa ɗe haɗetaake"},{"chapter":56,"verse":34,"text":"e danɗe ɓamtaaɗe"},{"chapter":56,"verse":35,"text":"Ko Menen tagi fuɗɗi ɓe"},{"chapter":56,"verse":36,"text":"Men waɗi ɓe curbaaɓe-boomi"},{"chapter":56,"verse":37,"text":"yiɗuɓe worɓe mun, goreeɓe"},{"chapter":56,"verse":38,"text":"[Men marani ɓe] yimɓe ñaamo ɓen"},{"chapter":56,"verse":39,"text":"Dental jeyaaɓe e [mofte] adinooɓe ɓen"},{"chapter":56,"verse":40,"text":"e seeɗaaɓe jeyaaɓe e [mofte] sakkitiiɓe ɓen"},{"chapter":56,"verse":41,"text":"E wontirooɓe nano ɓen. Ko homɓe woni ɓen yimɓe nano"},{"chapter":56,"verse":42,"text":"[Ɓen] no e nguleendi e fatayɗam"},{"chapter":56,"verse":43,"text":"e ɗowdi curki ɓawluki"},{"chapter":56,"verse":44,"text":"wonaa ɓuuɓundi, wanaa teddundi"},{"chapter":56,"verse":45,"text":"Ado ɗum, hari ko ɓe neeminaaɓe"},{"chapter":56,"verse":46,"text":"ɓe laatino hiɓe deppitoo e bakkaat mawɗi ɗin"},{"chapter":56,"verse":47,"text":"hiɓe wi'aynoo: \"Enee, si men maayii, men wontii mbullaari e ƴi'e, hara ko men immintinoyteeɓe"},{"chapter":56,"verse":48,"text":"hay baabiraaɓe amen adinooɓe ɓen"},{"chapter":56,"verse":49,"text":"Maaku : \"Pellet, aramɓe e sakkitiiɓe"},{"chapter":56,"verse":50,"text":"ko mooɓoyteeɓe e ñalaande anndaande waqutu mun"}]}