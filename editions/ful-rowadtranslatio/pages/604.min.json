{"pages":[{"chapter":112,"verse":1,"text":"Maaku ko Kanko woni Alla gooto"},{"chapter":112,"verse":2,"text":"Alla paandateeɗo"},{"chapter":112,"verse":3,"text":"O jibinaani O jinaaka"},{"chapter":112,"verse":4,"text":"O alah pasa hay gooto"},{"chapter":113,"verse":1,"text":"Maaku mbɗa moolo e jom subaka"},{"chapter":113,"verse":2,"text":"E bone ko o tagi"},{"chapter":113,"verse":3,"text":"E bone jemma niɓɓa kunu"},{"chapter":113,"verse":4,"text":"E bone wuttooɓe e piɓlee. @Corrected"},{"chapter":113,"verse":5,"text":"E bone haaside si o haasidiima"},{"chapter":114,"verse":1,"text":"Maaku mbeɗa moolo e jom yimɓe"},{"chapter":114,"verse":2,"text":"Laamɗo yimɓe"},{"chapter":114,"verse":3,"text":"Deweteeɗo yimɓe"},{"chapter":114,"verse":4,"text":"E bone seytaaneeli cikkinooji"},{"chapter":114,"verse":5,"text":"Oon cikkinoowo e ɓerɗe yimɓeɓɗj"},{"chapter":114,"verse":6,"text":"Ummaade e jinneji e yimɓe"}]}