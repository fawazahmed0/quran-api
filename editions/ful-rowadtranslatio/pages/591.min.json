{"pages":[{"chapter":86,"verse":1,"text":"O woondirii kammu e hoodere fuɗoore jemma"},{"chapter":86,"verse":2,"text":"Hol ko anndin ma ko woni hoodere feeñoore jemma"},{"chapter":86,"verse":3,"text":"Hoodere fooynunde"},{"chapter":86,"verse":4,"text":"Alah fittaandu si wonah ina e dow mayru deenoowo"},{"chapter":86,"verse":5,"text":"Yo aade ndaaru ko o tagraa ko"},{"chapter":86,"verse":6,"text":"O tagiraama ko e ndiyam nduñotoɗam"},{"chapter":86,"verse":7,"text":"Hombo e hakkunde keeci gorko e ƴi*e ɓernde debba"},{"chapter":86,"verse":8,"text":"Hombo hattani ruttude mbo"},{"chapter":86,"verse":9,"text":"ñande cuuɗiiɗi kuncaa"},{"chapter":86,"verse":10,"text":"Alanah mbo semmbe wonah balloowo"},{"chapter":86,"verse":11,"text":"Mi woondirii kammu jom ɗate ngun"},{"chapter":86,"verse":12,"text":"O woondirii leydi mbaɗndi petti"},{"chapter":86,"verse":13,"text":"Pellet o Quraana kombo ceerndoowol"},{"chapter":86,"verse":14,"text":"Mbo wonah gaajaate"},{"chapter":86,"verse":15,"text":"Pellet kamɓe hoɓe pewja peeje"},{"chapter":86,"verse":16,"text":"Mi fewja feere"},{"chapter":86,"verse":17,"text":"Muñanɓe muñangol setta"},{"chapter":87,"verse":1,"text":"Senin innde joom Ma ɓurnde toowde"},{"chapter":87,"verse":2,"text":"Oon takɗo O fonndi"},{"chapter":87,"verse":3,"text":"Oon koddirɗo refti O feewni"},{"chapter":87,"verse":4,"text":"Oon yaltinɗo durngol"},{"chapter":87,"verse":5,"text":"O watti ngol yoorko wayliiko"},{"chapter":87,"verse":6,"text":"Ma Min njanngin Ma A yeɓjjittah"},{"chapter":87,"verse":7,"text":"Si wonah ko Alla haaji pellet hombo anndi ko feeñi e ko wirnii"},{"chapter":87,"verse":8,"text":"Min newnan Ma na newori"},{"chapter":87,"verse":9,"text":"Siftinir ɗoon ɗo ciftanirgol na nafata"},{"chapter":87,"verse":10,"text":"Ma waacta oon kulɗo Alla"},{"chapter":87,"verse":11,"text":"Woɗɗitoo mbo ɓurɗo hiiteede"},{"chapter":87,"verse":12,"text":"Oon naatoowo yiite mannge nge"},{"chapter":87,"verse":13,"text":"Refti o maayatah toon o wuuratah"},{"chapter":87,"verse":14,"text":"Malaama oon laaɓɗo e sirku"},{"chapter":87,"verse":15,"text":"O jantii Innde joom makko o juuli"}]}