{"pages":[{"chapter":106,"verse":1,"text":"Sabu woowde Quraysi en"},{"chapter":106,"verse":2,"text":"Goowgol ɓe ɗatngal dabbunde e ceeɗu"},{"chapter":106,"verse":3,"text":"Yo ɓe ndew jom ndu suudu"},{"chapter":106,"verse":4,"text":"Oon ñamminɗo ɓe saanga heege O hoolni ɓe e kulol"},{"chapter":107,"verse":1,"text":"Mbela a anndii oon pennoowo njoɓdi ñande daranga"},{"chapter":107,"verse":2,"text":"E oon duñoowo alyatiime"},{"chapter":107,"verse":3,"text":"O reerɗiŋtaa e ñammiingol miskiino"},{"chapter":107,"verse":4,"text":"Halkaare woodanii juulooɓe"},{"chapter":107,"verse":5,"text":"Ɓen welsandiiɓe nder juulde maɓɓe"},{"chapter":107,"verse":6,"text":"ɓeen yiinatooɓe"},{"chapter":107,"verse":7,"text":"Hoɓe kaɗa ngenndukon"},{"chapter":108,"verse":1,"text":"Min tottiima AlKawsara. @Corrected"},{"chapter":108,"verse":2,"text":"juulan joom Ma mboroɗa"},{"chapter":108,"verse":3,"text":"Gañɗo ma o woni taƴiiɗo"}]}