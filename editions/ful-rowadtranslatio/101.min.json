{"chapter":[{"chapter":101,"verse":1,"text":"Darnga konngoowo ɓɗere"},{"chapter":101,"verse":2,"text":"Hol ko woni Qari*a"},{"chapter":101,"verse":3,"text":"Hol ko anndin Ma Qari*a"},{"chapter":101,"verse":4,"text":"ñande yimɓe mba*i no mbeɗon Alla carikon"},{"chapter":101,"verse":5,"text":"pelle ɗen wa'oya wa hottolle kardaako"},{"chapter":101,"verse":6,"text":"Tawi ko oon mbo peesirɗe mum teddi"},{"chapter":101,"verse":7,"text":"Hara himo e nguurndam welɗam"},{"chapter":101,"verse":8,"text":"Tawi ko oon mbo peesirɗe mum koyi"},{"chapter":101,"verse":9,"text":"Hoɗorde makko ko jahatnama"},{"chapter":101,"verse":10,"text":"Ko honɗum anndin maa ko woni ngeen"},{"chapter":101,"verse":11,"text":"Ko nge yiite wulnge"}]}