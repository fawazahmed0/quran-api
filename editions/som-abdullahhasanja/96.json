{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ku akhri Magaca Rabbigaa, ee abuuray"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Wuxuu ka abuuray aadmiga xinjir"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Akhri! Oo Rabbigaa waa Sharaf Badnaha"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ee baray sida wax loogu qoro qalinka"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Baray Aadmiga wax uusan aqoon"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Mayee! Aadmigu xaqiiq wuu xadgudbaa"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Inuu isu arko mid deeqtoon darteed"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Hubaal! Waa xagga Rabbigaa noqodku"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ma waad aragtay midka isaga ka reeba"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Addoon markuu tukado"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Bal ka warran haddii uu ku sugan yahay jid toosan"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Amaba uu amro xume ka dhowrsiga"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Bal ka warran haddii uu kaasina beeniyo Xaqa, oo dhabarka u jeediyo"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Mawuusan ogeyn in Allaah arko"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Mayee! Haddii uusan meel joogin, waxaanu hubaal qaban doonnaa foodda"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Food beenley ah oo gefley ah"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Markaa ha u yeedho golihiisa"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Waxaan innaguna u yeereynaa malaaâ€™ig daran"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Mayee, Ha adeecin isaga, oo sujuud, oo u dhowow (Allaah)"
        }
    ]
}