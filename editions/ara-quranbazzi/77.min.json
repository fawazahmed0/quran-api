{"chapter":[{"chapter":77,"verse":1,"text":"وَٱلۡمُرۡسَلَٰتِ عُرۡفࣰ ا"},{"chapter":77,"verse":2,"text":"فَٱلۡعَٰصِفَٰتِ عَصۡفࣰ ا"},{"chapter":77,"verse":3,"text":"وَٱلنَّٰشِرَٰتِ نَشۡرࣰ ا"},{"chapter":77,"verse":4,"text":"فَٱلۡفَٰرِقَٰتِ فَرۡقࣰ ا"},{"chapter":77,"verse":5,"text":"فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا"},{"chapter":77,"verse":6,"text":"عُذۡرًا أَوۡ نُذُرًا"},{"chapter":77,"verse":7,"text":"إِنَّمَا تُوعَدُونَ لَوَٰقِعࣱ‏"},{"chapter":77,"verse":8,"text":"فَإِذَا ٱلنُّجُومُ طُمِسَتۡ"},{"chapter":77,"verse":9,"text":"وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ"},{"chapter":77,"verse":10,"text":"وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ"},{"chapter":77,"verse":11,"text":"وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ"},{"chapter":77,"verse":12,"text":"لِأَيِّ يَوۡمٍ أُجِّلَتۡ"},{"chapter":77,"verse":13,"text":"لِيَوۡمِ ٱلۡفَصۡلِ"},{"chapter":77,"verse":14,"text":"وَمَا أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ"},{"chapter":77,"verse":15,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":16,"text":"أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ"},{"chapter":77,"verse":17,"text":"ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ"},{"chapter":77,"verse":18,"text":"كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ"},{"chapter":77,"verse":19,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":20,"text":"أَلَمۡ نَخۡلُقكُّمُۥ مِن مَّآءࣲ مَّهِينࣲ‏"},{"chapter":77,"verse":21,"text":"فَجَعَلۡنَٰهُۥ فِي قَرَارࣲ مَّكِينٍ"},{"chapter":77,"verse":22,"text":"إِلَىٰ قَدَرࣲ مَّعۡلُومࣲ‏"},{"chapter":77,"verse":23,"text":"فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ"},{"chapter":77,"verse":24,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":25,"text":"أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا"},{"chapter":77,"verse":26,"text":"أَحۡيَآءࣰ وَأَمۡوَٰتࣰ ا"},{"chapter":77,"verse":27,"text":"وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتࣲ وَأَسۡقَيۡنَٰكُمُۥ مَآءࣰ فُرَاتࣰ ا"},{"chapter":77,"verse":28,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":29,"text":"ٱنطَلِقُواْ إِلَىٰ مَا كُنتُمُۥ بِهِۦ تُكَذِّبُونَ"},{"chapter":77,"verse":30,"text":"ٱنطَلِقُواْ إِلَىٰ ظِلࣲّ ذِي ثَلَٰثِ شُعَبࣲ‏"},{"chapter":77,"verse":31,"text":"لَّا ظَلِيلࣲ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ"},{"chapter":77,"verse":32,"text":"إِنَّهَا تَرۡمِي بِشَرَرࣲ كَٱلۡقَصۡرِ"},{"chapter":77,"verse":33,"text":"كَأَنَّهُۥ جِمَٰلَٰتࣱ صُفۡرࣱ‏"},{"chapter":77,"verse":34,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":35,"text":"هَٰذَا يَوۡمُ لَا يَنطِقُونَ"},{"chapter":77,"verse":36,"text":"وَلَا يُؤۡذَنُ لَهُمُۥ فَيَعۡتَذِرُونَ"},{"chapter":77,"verse":37,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":38,"text":"هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمُۥ وَٱلۡأَوَّلِينَ"},{"chapter":77,"verse":39,"text":"فَإِن كَانَ لَكُمُۥ كَيۡدࣱ فَكِيدُونِ"},{"chapter":77,"verse":40,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":41,"text":"إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلࣲ وَعِيُونࣲ‏"},{"chapter":77,"verse":42,"text":"وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ"},{"chapter":77,"verse":43,"text":"كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمُۥ تَعۡمَلُونَ"},{"chapter":77,"verse":44,"text":"إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"},{"chapter":77,"verse":45,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":46,"text":"كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُمُۥ مُجۡرِمُونَ"},{"chapter":77,"verse":47,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":48,"text":"وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ"},{"chapter":77,"verse":49,"text":"وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"},{"chapter":77,"verse":50,"text":"فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ"}]}