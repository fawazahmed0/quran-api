{
    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلۡمِي بِمَا كَانُواْ يَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنۡ حِسَابُهُمُۥ إِلَّا عَلَىٰ رَبِّيۖ لَوۡ تَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَا أَنَا۠ بِطَارِدِ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنۡ أَنَا۠ إِلَّا نَذِير ࣱ مُّبِين ࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُواْ لَئِن لَّمۡ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ ٱلۡمَرۡجُومِينَ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوۡمِي كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَٱفۡتَحۡ بَيۡنِي وَبَيۡنَهُمُۥ فَتۡح ࣰ ا وَنَجِّنِي وَمَن مَّعِي مِنَ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَيۡنَٰهُۥ وَمَن مَّعَهُۥ فِي ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغۡرَقۡنَا بَعۡدُ ٱلۡبَاقِينَ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكۡثَرُهُمُۥ مُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَالَ لَهُمُۥ أَخُوهُمُۥ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّي لَكُمُۥ رَسُولٌ أَمِين ࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَا أَسۡـَٔلُكُمُۥ عَلَيۡهِۦ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِي إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِيعٍ ءَايَة ࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمُۥ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُمُۥ بَطَشۡتُمُۥ جَبَّارِينَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَٱتَّقُواْ ٱلَّذِي أَمَدَّكُمُۥ بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُمُۥ بِأَنۡعَٰم ࣲ وَبَنِينَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّٰت ࣲ وَعِيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّيَ أَخَافُ عَلَيۡكُمُۥ عَذَابَ يَوۡمٍ عَظِيم ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُواْ سَوَآءٌ عَلَيۡنَا أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ ٱلۡوَٰعِظِينَ"
        }
    ]
}