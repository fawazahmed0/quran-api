        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "هَٰذَا نُزُلُهُمُۥ يَوۡمَ ٱلدِّينِ"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "نَحۡنُ خَلَقۡنَٰكُمُۥ فَلَوۡلَا تُصَدِّقُونَ"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "أَفَرَءَيۡتُمُۥ مَا تُمۡنُونَ"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ءَا۬نتُمُۥ تَخۡلُقُونَهُۥ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "نَحۡنُ قَدَرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "عَلَىٰ أَن نُّبَدِّلَ أَمۡثَٰلَكُمُۥ وَنُنشِئَكُمُۥ فِي مَا لَا تَعۡلَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشَآءَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَّكَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "أَفَرَءَيۡتُمُۥ مَا تَحۡرُثُونَ"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ءَا۬نتُمُۥ تَزۡرَعُونَهُۥ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُۥ حُطَٰم ࣰ ا فَظَلۡتُمُۥ تَفَكَّهُونَ"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "إِنَّا لَمُغۡرَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
        },
        {
            "chapter": 56,
            "verse": 68,
